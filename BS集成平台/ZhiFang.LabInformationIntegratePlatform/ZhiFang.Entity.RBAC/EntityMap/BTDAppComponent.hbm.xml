    <property name="InitParameter" column="[InitParameter]" type="String" not-null="false" length="200" />
    <property name="AppType" column="[AppType]" type="Int32" not-null="false" length="4" />
    <property name="BuildType" column="[BuildType]" type="Int32" not-null="false" length="4" />
    <property name="ExecuteCode" column="[ExecuteCode]" type="String" not-null="false" length="1073741823" />
    <property name="DesignCode" column="[DesignCode]" type="String" not-null="false" length="1073741823" />
