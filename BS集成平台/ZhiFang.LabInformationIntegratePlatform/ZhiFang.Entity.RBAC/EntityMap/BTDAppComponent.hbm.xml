      <key>
        <column name="[AppComID]" />
      </key>
      <one-to-many class="BTDAppComponentsOperate"/>
    </bag>
    <bag name="BTDAppComponentsRefList"  inverse="false" cascade="save-update" lazy="true">
      <key>
        <column name="[AppComID]" />
      </key>
      <one-to-many class="BTDAppComponentsRef"/>
    </bag>
