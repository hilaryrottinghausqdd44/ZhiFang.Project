<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.LabStar" assembly="ZhiFang.Entity.LabStar">
  <class name="LBEquipSection" table="[dbo].[LB_EquipSection]">
    <id name="Id" column="[EquipSectionID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="CompValue1" column="[CompValue1]" type="String" not-null="false" length="50" />
    <property name="CompValue2" column="[CompValue2]" type="String" not-null="false" length="50" />
    <property name="SampleNoCode" column="[SampleNoCode]" type="String" not-null="false" length="200" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <many-to-one name="LBEquip" class="LBEquip" lazy="proxy">
      <column name="[EquipID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="LBItem" class="LBItem" lazy="proxy">
      <column name="[ItemID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="LBSection" class="LBSection" lazy="proxy">
      <column name="[SectionID]" not-null="false"/>
    </many-to-one>
  </class>
</hibernate-mapping>