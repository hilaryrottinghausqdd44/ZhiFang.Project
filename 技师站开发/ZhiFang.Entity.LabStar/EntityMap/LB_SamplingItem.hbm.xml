<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.LabStar" assembly="ZhiFang.Entity.LabStar">
  <class name="LBSamplingItem" table="[dbo].[LB_SamplingItem]">
    <id name="Id" column="[SamplingItemID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="IsDefault" column="[IsDefault]" type="Boolean" not-null="false" length="1" />
    <property name="MustItemID" column="[MustItemID]" type="Int64" not-null="false" length="8" />
    <property name="VirtualItemNo" column="[VirtualItemNo]" type="Int32" not-null="false" length="4" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <many-to-one name="LBItem" class="LBItem" lazy="proxy">
      <column name="[ItemID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="LBSamplingGroup" class="LBSamplingGroup" lazy="proxy">
      <column name="[SamplingGroupID]" not-null="false"/>
    </many-to-one>
  </class>
</hibernate-mapping>
