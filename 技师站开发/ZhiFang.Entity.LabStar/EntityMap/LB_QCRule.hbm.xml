<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.LabStar" assembly="ZhiFang.Entity.LabStar">
	<class name="LBQCRule" table="[dbo].[LB_QCRule]">
		<id name="Id" column="[QCRuleID]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
		<property name="QCRuleName" column="[QCRuleName]" type="String" not-null="false" length="100" />
		<property name="RuleDesc" column="[RuleDesc]" type="String" not-null="false" length="200" />
		<property name="BDefault" column="[bDefault]" type="Boolean" not-null="false" length="1" />
		<property name="BWestGard" column="[bWestGard]" type="Boolean" not-null="false" length="1" />
		<property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
		<property name="LoseType" column="[LoseType]" type="String" not-null="false" length="20" />
		<property name="BWarnState" column="[bWarnState]" type="Boolean" not-null="false" length="1" />
		<property name="Operator" column="[Operator]" type="String" not-null="false" length="50" />
		<property name="OperatorID" column="[OperatorID]" type="Int64" not-null="false" length="8" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
		<bag name="LBQCItemRuleList" inverse="true" cascade="all-delete-orphan" lazy="true">
			<key>
                <column name="[QCRuleID]" />
            </key>
			<one-to-many class="LBQCItemRule"/>
		</bag>
		<bag name="LBQCRulesConList" inverse="true" cascade="all-delete-orphan" lazy="true">
			<key>
                <column name="[QCRuleID]" />
            </key>
			<one-to-many class="LBQCRulesCon"/>
		</bag>
	</class>
</hibernate-mapping>
