<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.OA" assembly="ZhiFang.Entity.OA">
	<class name="BIcons" table="[dbo].[B_Icons]">
		<id name="Id" column="[IconsID]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="IconsTypeID" column="[IconsTypeID]" type="Int64" not-null="false" length="8" />
		<property name="Width" column="[Width]" type="Int32" not-null="false" length="4" />
		<property name="Height" column="[Height]" type="Int32" not-null="false" length="4" />
		<property name="Size" column="[Size]" type="Int64" not-null="false" length="8" />
		<property name="Url" column="[Url]" type="String" not-null="true" length="100" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="true" length="8" />
		<property name="IsLocal" column="[IsLocal]" type="Boolean" not-null="true" length="1" />
		<property name="Comment" column="[Comment]" type="String" not-null="false" length="300" />
		<bag name="BWeiXinAccountList" inverse="true" cascade="all-delete-orphan" lazy="true">
			<key>
                <column name="[IconsID]" />
            </key>
			<one-to-many class="BWeiXinAccount"/>
		</bag>
	</class>
</hibernate-mapping>
