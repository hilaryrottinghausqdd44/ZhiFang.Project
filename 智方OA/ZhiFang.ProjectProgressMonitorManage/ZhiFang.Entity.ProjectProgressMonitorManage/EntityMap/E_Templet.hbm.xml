<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.ProjectProgressMonitorManage" assembly="ZhiFang.Entity.ProjectProgressMonitorManage">
  <class name="ETemplet" table="[dbo].[E_Templet]">
    <id name="Id" column="[TempletID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="false" length="8" />
    <property name="UseCode" column="[UseCode]" type="String" not-null="false" length="50" />
    <property name="CName" column="[CName]" type="String" not-null="true" length="200" />
    <property name="EName" column="[EName]" type="String" not-null="false" length="200" />
    <property name="SName" column="[SName]" type="String" not-null="false" length="100" />
    <property name="Shortcode" column="[Shortcode]" type="String" not-null="false" length="50" />
    <property name="PinYinZiTou" column="[PinYinZiTou]" type="String" not-null="false" length="50" />
    <property name="TempletPath" column="[TempletPath]" type="String" not-null="false" length="1000" />
    <property name="TempletStruct" column="[TempletStruct]" type="String" not-null="false" length="8000" />
    <property name="TempletFillStruct" column="[TempletFillStruct]" type="String" not-null="false" length="8000" />
    <property name="FillStruct" column="[FillStruct]" type="String" not-null="false" length="8000" />
    <property name="BeginDate" column="[BeginDate]" type="DateTime" not-null="false" length="8" />
    <property name="EndDate" column="[EndDate]" type="DateTime" not-null="false" length="8" />
    <property name="Comment" column="[Comment]" type="String" not-null="false" length="8000" />
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="CheckType" column="[CheckType]" type="Int32" not-null="false" length="4" />
    <property name="FillType" column="[FillType]" type="Int32" not-null="false" length="4" />
    <property name="ShowFillItem" column="[ShowFillItem]" type="String" not-null="false" length="500" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <many-to-one name="EEquip" class="EEquip" lazy="false">
      <column name="[EquipID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="TempletType" class="PDict" lazy="false">
      <column name="[TempletTypeID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="Section" class="ZhiFang.Entity.RBAC.HRDept" lazy="false">
      <column name="[SectionID]" not-null="false"/>
    </many-to-one>
    <bag name="EMaintenanceDataList" inverse="true" cascade="save-update" lazy="true">
      <key>
        <column name="[TempletID]" />
      </key>
      <one-to-many class="EMaintenanceData"/>
    </bag>
    <bag name="EReportDataList" inverse="true" cascade="save-update" lazy="true">
      <key>
        <column name="[TempletID]" />
      </key>
      <one-to-many class="EReportData"/>
    </bag>
    <bag name="ETempletEmpList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[TempletID]" />
      </key>
      <one-to-many class="ETempletEmp"/>
    </bag>
  </class>
</hibernate-mapping>
