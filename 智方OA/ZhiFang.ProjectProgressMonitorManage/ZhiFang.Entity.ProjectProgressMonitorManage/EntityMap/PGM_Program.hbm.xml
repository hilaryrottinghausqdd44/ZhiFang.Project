    <property name="Content" column="[Content]" type="String" not-null="false" />
    <property name="Status" column="[Status]" type="Int32" not-null="false" length="4" />
    <property name="Keyword" column="[Keyword]" type="String" not-null="false" length="100" />
    <property name="VersionNo" column="[VersionNo]" type="String" not-null="false" length="100" />
    <property name="Memo" column="[Memo]" type="String" not-null="false" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="CreatorID" column="[CreatorID]" type="Int64" not-null="false" length="8" />
    <property name="CreatorName" column="[CreatorName]" type="String" not-null="false" length="50" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <!--
    <property name="OriginalFileID" column="[OriginalFileID]" type="Int64" not-null="false" length="8" />
    -->
    <property name="PublisherID" column="[PublisherID]" type="Int64" not-null="false" length="8" />
    <property name="PublisherName" column="[PublisherName]" type="String" not-null="false" length="50" />
    <property name="Counts" column="[Counts]" type="Int32" not-null="false" length="4" />
    <property name="IsDiscuss" column="[IsDiscuss]" type="Boolean" not-null="false" length="1" />
    <property name="PublisherDateTime" column="[PublisherDateTime]" type="DateTime" not-null="false" length="8" />
    <property name="Size" column="[Size]" type="Int64" not-null="false" length="8" />
    <property name="FileName" column="[FileName]" type="String" not-null="false" length="500" />
    <property name="FileExt" column="[FileExt]" type="String" not-null="false" length="100" />
    <property name="FilePath" column="[FilePath]" type="String" not-null="false" length="500" />
    <property name="NewFileName" column="[NewFileName]" type="String" not-null="false" length="1000" />
    <property name="FileType" column="[FileType]" type="String" not-null="false" length="100" />
