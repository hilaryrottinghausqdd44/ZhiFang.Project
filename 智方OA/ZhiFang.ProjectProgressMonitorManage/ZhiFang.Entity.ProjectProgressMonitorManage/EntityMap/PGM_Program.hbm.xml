    <property name="ClientName" column="[ClientName]" type="String" not-null="false" length="200" />
    <property name="OtherFactoryID" column="[OtherFactoryID]" type="Int64" not-null="false" length="8" />
    <property name="OtherFactoryName" column="[OtherFactoryName]" type="String" not-null="false" length="200" />
    <property name="SQH" column="[SQH]" type="String" not-null="false" length="100" />
    <many-to-one name="BEquip" class="BEquip" >
      <column name="[EquipID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="PBDictTree" class="BDictTree" >
      <column name="[PTypeTreeID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="SubBDictTree" class="BDictTree" >
      <column name="[SubTypeTreeID]" not-null="false"/>
    </many-to-one>

    <many-to-one name="OriginalPGMProgram" class="PGMProgram" >
      <column name="[OriginalFileID]" not-null="false"/>
    </many-to-one>
  </class>
