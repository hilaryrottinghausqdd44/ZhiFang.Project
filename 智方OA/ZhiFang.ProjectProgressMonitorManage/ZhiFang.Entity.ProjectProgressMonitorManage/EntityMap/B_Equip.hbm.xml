<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.ProjectProgressMonitorManage" assembly="ZhiFang.Entity.ProjectProgressMonitorManage">
  <class name="BEquip" table="[dbo].[B_Equip]">
    <id name="Id" column="[EquipID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="false" length="8" />
    <property name="CName" column="[CName]" type="String" not-null="true" length="500" />
    <property name="EName" column="[EName]" type="String" not-null="false" length="500" />
    <property name="SName" column="[SName]" type="String" not-null="false" length="500" />
    <property name="Shortcode" column="[Shortcode]" type="String" not-null="false" length="500" />
    <property name="PinYinZiTou" column="[PinYinZiTou]" type="String" not-null="false" length="500" />
    <property name="UseCode" column="[UseCode]" type="String" not-null="false" length="500" />
    <property name="Comment" column="[Comment]" type="String" not-null="false" length="4000" />
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <property name="Equipversion" column="[Equipversion]" type="String" not-null="false" length="500" />
    <property name="Memo" column="[Memo]" type="String" not-null="false" />
    <property name="Content" column="[Content]" type="String" not-null="false" />
    <property name="FullCName" column="[FullCName]" type="String" not-null="false" length="500" />
    <many-to-one name="EquipFactoryBrand" class="PDict">
      <column name="[EquipFactoryBrandID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="EquipType" class="PDict">
      <column name="[EquipTypeID]" not-null="false"/>
    </many-to-one>
    <bag name="PGMProgramList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[EquipID]" />
      </key>
      <one-to-many class="PGMProgram"/>
    </bag>
  </class>
</hibernate-mapping>
