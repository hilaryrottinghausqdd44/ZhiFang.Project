<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.ProjectProgressMonitorManage" assembly="ZhiFang.Entity.ProjectProgressMonitorManage">
	<class name="FFileAttachment" table="[dbo].[F_File_Attachment]">
		<id name="Id" column="[FileAttachmentID]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
		<property name="FileName" column="[FileName]" type="String" not-null="false" length="500" />
		<property name="FileExt" column="[FileExt]" type="String" not-null="false" length="100" />
    
    <property name="NewFileName" column="[NewFileName]" type="String" not-null="false" length="500" />
    <property name="FileType" column="[FileType]" type="String" not-null="false" length="100" />
    
		<property name="FileSize" column="[FileSize]" type="Int64" not-null="false" length="8" />
		<property name="FilePath" column="[FilePath]" type="String" not-null="false" length="500" />
		<property name="Memo" column="[Memo]" type="String" not-null="false" length="500" />
		<property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
		<property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
		<property name="CreatorName" column="[CreatorName]" type="String" not-null="false" length="50" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
		<many-to-one name="FFile" class="FFile" >
			<column name="[FileID]" not-null="false"/>
		</many-to-one>
		<many-to-one name="Creator" class="ZhiFang.Entity.RBAC.HREmployee" >
			<column name="[CreatorID]" not-null="false"/>
		</many-to-one>
	</class>
</hibernate-mapping>
