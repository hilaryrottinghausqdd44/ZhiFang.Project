<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.ProjectProgressMonitorManage" assembly="ZhiFang.Entity.ProjectProgressMonitorManage">
  <class name="PProjectDocument" table="[dbo].[P_ProjectDocument]">
    <id name="Id" column="[ProjectDocumentID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="false" length="8" />
    <property name="ProjectTaskID" column="[ProjectTaskID]" type="Int64" not-null="false" length="8" />
    <property name="DocumentName" column="[DocumentName]" type="String" not-null="false" length="500" />
    <property name="DocumentLink" column="[DocumentLink]" type="String" not-null="false" length="500" />
    <property name="Content" column="[Content]" type="String" not-null="false" />
    <property name="Memo" column="[Memo]" type="String" not-null="false" length="500" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="FileDateTime" column="[FileDateTime]" type="DateTime" not-null="false" length="8" />
    <many-to-one name="Creater" class="ZhiFang.Entity.RBAC.HREmployee" lazy="false">
      <column name="[CreatEmpID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="PProject" class="PProject" lazy="false">
      <column name="[ProjectID]" not-null="false"/>
    </many-to-one>
  </class>
</hibernate-mapping>
