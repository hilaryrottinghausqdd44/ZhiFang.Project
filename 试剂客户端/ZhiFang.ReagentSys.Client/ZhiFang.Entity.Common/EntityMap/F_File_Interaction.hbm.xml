<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.Common" assembly="ZhiFang.Entity.Common">
	<class name="FFileInteraction" table="[dbo].[F_File_Interaction]">
		<id name="Id" column="[InteractionID]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="false" length="8" />
		<property name="Contents" column="[Contents]" type="String" not-null="false" />
		<property name="SenderName" column="[SenderName]" type="String" not-null="false" length="50" />
		<property name="ReceiverName" column="[ReceiverName]" type="String" not-null="false" length="50" />
		<property name="HasAttachment" column="[HasAttachment]" type="Boolean" not-null="false" length="1" />
		<property name="Memo" column="[Memo]" type="String" not-null="false" length="500" />
		<property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="true" length="8" />
    <property name="SenderID" column="[SenderID]" type="Int64" not-null="true" length="8" />
		<many-to-one name="FFile" class="FFile" >
			<column name="[FileID]" not-null="false"/>
		</many-to-one>
		<many-to-one name="Receiver" class="ZhiFang.Entity.RBAC.HREmployee" >
			<column name="[ReceiverID]" not-null="false"/>
		</many-to-one>
		
	</class>
</hibernate-mapping>
