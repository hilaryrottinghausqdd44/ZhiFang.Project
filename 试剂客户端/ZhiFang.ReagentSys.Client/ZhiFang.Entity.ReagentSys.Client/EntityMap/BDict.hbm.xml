<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.ReagentSys.Client" assembly="ZhiFang.Entity.ReagentSys.Client">
  <class name="BDict" table="[dbo].[B_Dict]">
    <id name="Id" column="[DID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="CName" column="[CName]" type="String" not-null="false" length="80" />
    <property name="SName" column="[SName]" type="String" not-null="false" length="80" />
    <property name="Shortcode" column="[Shortcode]" type="String" not-null="false" length="40" />
    <property name="PinYinZiTou" column="[PinYinZiTou]" type="String" not-null="false" length="50" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="Memo" column="[Memo]" type="String" not-null="false" length="12000" />
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="MatchCode" column="[MatchCode]" type="String" not-null="false" length="100" />    

    <many-to-one name="BDictType" class="BDictType" >
      <column name="[DCId]" not-null="false"/>
    </many-to-one>
    <!--<bag name="BParameterList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[PDictId]" />
      </key>
      <one-to-many class="BParameter"/>
    </bag>-->
  </class>
</hibernate-mapping>
