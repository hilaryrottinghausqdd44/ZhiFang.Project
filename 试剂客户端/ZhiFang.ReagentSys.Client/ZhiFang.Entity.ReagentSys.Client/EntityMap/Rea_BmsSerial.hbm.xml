<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.ReagentSys.Client" assembly="ZhiFang.Entity.ReagentSys.Client">
	<class name="ReaBmsSerial" table="[dbo].[Rea_BmsSerial]">
		<id name="Id" column="[RulesId]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="false" length="8" />
		<property name="BmsType" column="[BmsType]" type="String" not-null="false" length="60" />
		<property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<property name="CurBarCode" column="[CurBarCode]" type="Int64" not-null="false" length="6" />
	</class>
  <!--按LabID加条码类型,获取一维条码的当前最大序号-->
  <sql-query name="P_GetReaBmsSerial">
    <return class="ZhiFang.Entity.ReagentSys.Client.ReaBmsSerial,ZhiFang.Entity.ReagentSys.Client">
      <return-property name="Id" column="[RulesId]" />
      <return-property name="LabID" column="[LabID]" />
      <return-property name="BmsType" column="[BmsType]" />
      <return-property name="DataAddTime" column="[DataAddTime]" />
      <return-property name="DataUpdateTime" column="[DataUpdateTime]" />
      <return-property name="CurBarCode" column="[CurBarCode]" />
    </return>
    exec P_GetReaBmsSerial :rulesId,:labId,:bmsType
  </sql-query>
</hibernate-mapping>
