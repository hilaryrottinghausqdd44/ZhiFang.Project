<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.BloodTransfusion" assembly="ZhiFang.Entity.BloodTransfusion">
  <class name="BloodBagProcess" table="[dbo].[Blood_BagProcess]">
    <id name="Id" column="[BPID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="BBagCode" column="[BBagCode]" type="String" not-null="false" length="20" />
    <property name="PCode" column="[PCode]" type="String" not-null="false" length="20" />
    <property name="B3Code" column="[B3Code]" type="String" not-null="false" length="50" />
    <property name="BPflag" column="[BPflag]" type="Int32" not-null="false" length="4" />

    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <!--
     <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="BPreFormID" column="[BPreFormID]" type="String" not-null="false" length="50" />
    <property name="BPreItemID" column="[BPreItemID]" type="String" not-null="false" length="50" />
    <property name="PTNo" column="[PTNo]" type="String" not-null="false" length="20" />
    -->
    <many-to-one name="BloodBPreForm" class="BloodBPreForm" lazy="proxy" >
      <column name="[BPreFormID]" not-null="true"/>
    </many-to-one>
    <many-to-one name="BloodBPreItem" class="BloodBPreItem" lazy="proxy" >
      <column name="[BPreItemID]" not-null="true"/>
    </many-to-one>
    <many-to-one name="BloodBagProcessType" class="BloodBagProcessType" lazy="proxy" >
      <column name="[PTNo]" not-null="true"/>
    </many-to-one>
  
  </class>
</hibernate-mapping>
