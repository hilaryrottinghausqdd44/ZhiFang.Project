<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.BloodTransfusion" assembly="ZhiFang.Entity.BloodTransfusion">
  <class name="BloodTransOperation" table="[dbo].[Blood_TransOperation]">
    <id name="Id" column="[OperationID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="false" length="8" />
    <property name="ContentTypeID" column="[ContentTypeID]" type="Int32" not-null="false" length="4" />
    <property name="BusinessCode" column="[BusinessCode]" type="String" not-null="false" length="40" />
    <property name="Memo" column="[Memo]" type="String" not-null="false" length="5000" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="CreatorID" column="[CreatorID]" type="Int64" not-null="false" length="8" />
    <property name="CreatorName" column="[CreatorName]" type="String" not-null="false" length="50" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />

    <many-to-one name="BloodBOutItem" class="BloodBOutItem" lazy="proxy" >
      <column name="[BOutItemID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="Bloodstyle" class="Bloodstyle" lazy="proxy" >
      <column name="[BloodNo]" not-null="false"/>
    </many-to-one>
    <many-to-one name="BloodTransForm" class="BloodTransForm" lazy="proxy" >
      <column name="[TransFormID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="BloodTransRecordType" class="BloodTransRecordType" lazy="proxy" >
      <column name="[TransRecordTypeID]" not-null="false"/>
    </many-to-one>
  </class>
</hibernate-mapping>
