<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.BloodTransfusion" assembly="ZhiFang.Entity.BloodTransfusion">
  <class name="BloodBTestItem" table="[dbo].[Blood_BTestItem]">
    <id name="Id" column="[BTestItemNo]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="false" length="8" />
    <property name="BTestItemName" column="[BTestItemName]" type="String" not-null="false" length="50" />
    <property name="EName" column="[EName]" type="String" not-null="false" length="20" />
    <property name="SName" column="[SName]" type="String" not-null="false" length="50" />
    <property name="PinYinZiTou" column="[PinYinZiTou]" type="String" not-null="false" length="50" />
    <property name="ShortCode" column="[ShortCode]" type="String" not-null="false" length="20" />
    <property name="IsGroup" column="[IsGroup]" type="Boolean" not-null="false" length="1" />
    <!--<property name="HisOrderCode" column="[HisOrderCode]" type="String" not-null="false" length="20" />-->
    <property name="Reference" column="[Reference]" type="String" not-null="false" length="200" />
    <property name="IsResultItem" column="[IsResultItem]" type="Boolean" not-null="false" length="1" />
    <property name="IsPreTransEvalItem" column="[IsPreTransEvalItem]" type="Boolean" not-null="false" length="1" />
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <many-to-one name="BloodBUnit" class="BloodBUnit" lazy="proxy" >
      <column name="[BUnitNo]" not-null="false"/>
    </many-to-one>
  </class>
</hibernate-mapping>
