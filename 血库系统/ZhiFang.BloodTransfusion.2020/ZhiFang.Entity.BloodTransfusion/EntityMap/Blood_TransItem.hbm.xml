<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.BloodTransfusion" assembly="ZhiFang.Entity.BloodTransfusion">
	<class name="BloodTransItem" table="[dbo].[Blood_TransItem]">
		<id name="Id" column="[TransItemID]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="false" length="8" />
		<property name="ContentTypeID" column="[ContentTypeID]" type="Int32" not-null="false" length="4" />
		<property name="TransItemResult" column="[TransItemResult]" type="String" not-null="false" length="200" />
		<property name="NumberItemResult" column="[NumberItemResult]" type="Double" not-null="false" length="8" />
		<property name="Visible" column="[Visible]" type="Boolean" not-null="false" length="1" />
		<property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<many-to-one name="BloodBagRecordType" class="BloodBagRecordType" lazy="proxy" >
			<column name="[TransRecordTypeID]" not-null="false"/>
		</many-to-one>
		<many-to-one name="BloodBagRecordItem" class="BloodBagRecordItem" lazy="proxy" >
			<column name="[TransRecordTypeItemID]" not-null="false"/>
		</many-to-one>
		<many-to-one name="BloodTransForm" class="BloodTransForm" lazy="proxy" >
			<column name="[TransFormID]" not-null="true"/>
		</many-to-one>
	</class>
</hibernate-mapping>
