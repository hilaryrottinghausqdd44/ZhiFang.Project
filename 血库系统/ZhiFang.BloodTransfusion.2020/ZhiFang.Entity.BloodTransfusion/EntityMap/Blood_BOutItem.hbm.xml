<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.BloodTransfusion" assembly="ZhiFang.Entity.BloodTransfusion">
  <class name="BloodBOutItem" table="[dbo].[Blood_BOutItem]">
    <id name="Id" column="[BOutItemID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="false" length="8" />
    <property name="BOutItemNo" column="[BOutItemNo]" type="String" not-null="false" length="20" />
    <property name="CheckFlag" column="[CheckFlag]" type="Int32" not-null="false" length="4" />
    <property name="BODate" column="[BODate]" type="DateTime" not-null="false" length="8" />
    <property name="OutFlag" column="[OutFlag]" type="Int32" not-null="false" length="4" />
    <property name="ConfirmCompletion" column="[ConfirmCompletion]" type="Int32" not-null="false" length="4" />
    <property name="HandoverCompletion" column="[HandoverCompletion]" type="Int32" not-null="false" length="4" />
    <property name="CourseCompletion" column="[CourseCompletion]" type="Int32" not-null="false" length="4" />
    <property name="RecoverCompletion" column="[RecoverCompletion]" type="Int32" not-null="false" length="4" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="Visible" column="[Visible]" type="Boolean" not-null="false" length="1" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <many-to-one name="BloodBOutForm" class="BloodBOutForm" lazy="proxy" >
      <column name="[BOutFormID]" not-null="true"/>
    </many-to-one>
    <many-to-one name="BloodQtyDtl" class="BloodQtyDtl" lazy="proxy" >
      <column name="[QtyDtlID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="BloodStyle" class="BloodStyle" lazy="proxy" >
      <column name="[BloodNo]" not-null="false"/>
    </many-to-one>
  </class>
</hibernate-mapping>
