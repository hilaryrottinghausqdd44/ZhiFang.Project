<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.BloodTransfusion" assembly="ZhiFang.Entity.BloodTransfusion">
  <class name="BloodChargeItem" table="[dbo].[Blood_ChargeItem]">
    <id name="Id" column="[ChargeItemNo]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="false" length="8" />
    <property name="CName" column="[ChargeItemName]" type="String" not-null="false" length="100" />
    <property name="SName" column="[SName]" type="String" not-null="false" length="50" />
    <property name="ShortCode" column="[ShortCode]" type="String" not-null="false" length="50" />
    <property name="PinYinZiTou" column="[PinYinZiTou]" type="String" not-null="false" length="50" />
    <property name="IsGroup" column="[IsGroup]" type="Boolean" not-null="false" length="1" />
    <property name="Modulus" column="[Modulus]" type="Double" not-null="false" length="8" />
    <property name="InPrice" column="[InPrice]" type="Decimal" not-null="false" length="8" />
    <property name="OutPrice" column="[OutPrice]" type="Decimal" not-null="false" length="8" />
    <!--<property name="HisOrderCode" column="[HisOrderCode]" type="String" not-null="false" length="20" />-->
    <property name="HisChargeItemUnits" column="[HisChargeItemUnits]" type="String" not-null="false" length="20" />
    <property name="HisPriceDemo" column="[HisPriceDemo]" type="String" not-null="false" length="20" />
    <property name="ChargeItemSpec" column="[ChargeItemSpec]" type="String" not-null="false" length="200" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <many-to-one name="BloodChargeItemType" class="BloodChargeItemType" lazy="proxy" >
      <column name="[ChargeItemTypeNo]" not-null="false"/>
    </many-to-one>
  </class>
</hibernate-mapping>
