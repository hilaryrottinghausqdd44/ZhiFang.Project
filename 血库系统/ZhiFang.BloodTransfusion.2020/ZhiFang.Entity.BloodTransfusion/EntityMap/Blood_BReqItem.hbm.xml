<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.BloodTransfusion" assembly="ZhiFang.Entity.BloodTransfusion">
  <class name="BloodBReqItem" table="[dbo].[Blood_BReqItem]">
    <id name="Id" column="[BReqItemID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="false" length="8" />
    <property name="BReqItemNo" column="[BReqItemNo]" type="String" not-null="false" length="20" />
    <property name="BReqCount" column="[BReqCount]" type="Double" not-null="false" length="8" />
    <property name="OtherItemNo" column="[OtherItemNo]" type="String" not-null="false" length="50" />
    <property name="Memo" column="[Memo]" type="String" not-null="false" length="500" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="Visible" column="[Visible]" type="Boolean" not-null="false" length="1" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <many-to-one name="BloodBReqForm" class="BloodBReqForm" lazy="proxy" >
      <column name="[BReqFormID]" not-null="true"/>
    </many-to-one>
    <many-to-one name="BloodClass" class="BloodClass" lazy="proxy" >
      <column name="[BCNo]" not-null="false"/>
    </many-to-one>
    <many-to-one name="BloodStyle" class="BloodStyle" lazy="proxy" >
      <column name="[BloodNo]" not-null="true"/>
    </many-to-one>
    <many-to-one name="BloodUnit" class="BloodUnit" lazy="proxy" >
      <column name="[BloodUnitNo]" not-null="false"/>
    </many-to-one>
    
  </class>
</hibernate-mapping>
