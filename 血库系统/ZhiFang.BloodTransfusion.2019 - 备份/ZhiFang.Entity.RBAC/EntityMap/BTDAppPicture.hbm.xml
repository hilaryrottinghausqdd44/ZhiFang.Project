<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.RBAC" assembly="ZhiFang.Entity.RBAC">
  <class name="BTDAppPicture" table="[dbo].[BT_D_AppPicture]">
    <id name="Id" column="[AppPictureID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="CName" column="[CName]" type="String" not-null="false" length="50" />
    <property name="FileSize" column="[FileSize]" type="Int32" not-null="false" length="4" />
    <property name="ShowSize" column="[ShowSize]" type="String" not-null="false" length="20" />
    <property name="PictureFormat" column="[PictureFormat]" type="String" not-null="false" length="10" />
    <property name="PictureURL" column="[PictureURL]" type="String" not-null="false" length="100" />
    <property name="Creator" column="[Creator]" type="String" not-null="false" length="20" />
    <property name="Modifier" column="[Modifier]" type="String" not-null="false" length="20" />
    <property name="PinYinZiTou" column="[PinYinZiTou]" type="String" not-null="false" length="50" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <bag name="BTDPictureTypeConsList" inverse="true" cascade="save-update" lazy="true">
      <key>
        <column name="[AppPictureID]" />
      </key>
      <one-to-many class="BTDPictureTypeCon"/>
    </bag>
  </class>
</hibernate-mapping>
