<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.RBAC" assembly="ZhiFang.Entity.RBAC">
	<class name="SLog" table="S_Log">
		<id name="Id" type="Int64" unsaved-value="null">
			<column name="LogID" length="8" sql-type="bigint" not-null="true" unique="true"/>
      <generator class="assigned" />
		</id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" type="Int64">
			<column name="LabID" length="8" sql-type="bigint" not-null="true"/>
		</property>
		<property name="OperateName" type="String">
			<column name="OperateName" length="512" sql-type="varchar" not-null="true"/>
		</property>
		<property name="OperateType" type="String">
			<column name="OperateType" length="200" sql-type="varchar" not-null="true"/>
		</property>
		<property name="IP" type="String">
			<column name="IP" length="50" sql-type="varchar" not-null="true"/>
		</property>
		<property name="InfoLevel" type="Int32">
			<column name="InfoLevel" length="4" sql-type="int" not-null="true"/>
		</property>
		<property name="Comment" type="String">
			<column name="Comment" sql-type="ntext" not-null="false"/>
		</property>
		<property name="DataAddTime" type="DateTime">
			<column name="DataAddTime" length="8" sql-type="datetime" not-null="false"/>
		</property>   
    <property name="EmpID" type="Int64">
      <column name="EmpID" length="8" sql-type="bigint" not-null="true"/>
    </property>
    <property name="EmpName" type="String">
      <column name="EmpName" length="50" sql-type="varchar" not-null="true"/>
    </property>
	</class>
</hibernate-mapping>
