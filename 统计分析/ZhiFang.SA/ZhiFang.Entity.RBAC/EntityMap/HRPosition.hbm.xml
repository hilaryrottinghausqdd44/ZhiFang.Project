<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.RBAC" assembly="ZhiFang.Entity.RBAC">
    <class name="HRPosition" table="[dbo].[HR_Position]" >
        <id name="Id" column="[PositionID]" type="Int64" >
            <generator class="assigned" />
        </id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
        <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" /> 
        <property name="Grade" column="[Grade]" type="Int32" not-null="true" /> 
        <property name="UseCode" column="[UseCode]" type="AnsiString" not-null="false" length="50" /> 
        <property name="StandCode" column="[StandCode]" type="AnsiString" not-null="false" length="50" /> 
        <property name="DeveCode" column="[DeveCode]" type="AnsiString" not-null="false" length="50" /> 
        <property name="CName" column="[CName]" type="String" not-null="false" length="50" /> 
        <property name="EName" column="[EName]" type="AnsiString" not-null="false" length="50" /> 
        <property name="SName" column="[SName]" type="AnsiString" not-null="false" length="50" /> 
        <property name="Shortcode" column="[Shortcode]" type="AnsiString" not-null="false" length="20" /> 
        <property name="Comment" column="[Comment]" type="String" not-null="false" length="16" /> 
        <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" /> 
        <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" /> 
        <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" /> 
        <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" /> 
        <bag name="HREmployeeList" inverse="true" cascade="save-update" lazy="true" >
            <key>
                <column name="[PositionID]" />
            </key>
            <one-to-many class="HREmployee" />
        </bag>
    </class>
</hibernate-mapping>

