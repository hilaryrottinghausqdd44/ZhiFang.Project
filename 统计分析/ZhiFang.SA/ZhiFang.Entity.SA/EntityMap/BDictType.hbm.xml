<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.SA" assembly="ZhiFang.Entity.SA">
  <class name="BDictType" table="[dbo].[B_DictType]">
    <id name="Id" column="[DCId]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="CName" column="[CName]" type="String" not-null="false" length="40" />
    <property name="DictTypeCode" column="[DictTypeCode]" type="String" not-null="true" length="100"  />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="Memo" column="[Memo]" type="String" not-null="false" length="12000" />
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <bag name="BDictList" inverse="true" cascade="all-delete-orphan" lazy="false">
      <key>
        <column name="[DCId]" />
      </key>
      <one-to-many class="BDict"/>
    </bag>
  </class>
</hibernate-mapping>
