      <column name="[FTypeTreeId]" not-null="false"/>
    </many-to-one>
    <many-to-one name="Creator" class="ZhiFang.Entity.RBAC.HREmployee" >
      <column name="[CreatorID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="NextExecutor" class="ZhiFang.Entity.RBAC.HREmployee" >
      <column name="[NextExecutorID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="Revisor" class="ZhiFang.Entity.RBAC.HREmployee" >
      <column name="[RevisorID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="ContentType" class="ZhiFang.Entity.SA.BDict" >
      <column name="[ContentType]" not-null="false"/>
    </many-to-one>
    <bag name="FFileAttachmentList" inverse="true" lazy="true">
      <key>
        <column name="[FileID]" />
      </key>
      <one-to-many class="FFileAttachment"/>
    </bag>
    <bag name="FFileCopyUserList" inverse="true" lazy="true">
      <key>
        <column name="[FileID]" />
      </key>
      <one-to-many class="FFileCopyUser"/>
    </bag>
    <bag name="FFileInteractionList" inverse="true" lazy="true">
      <key>
        <column name="[FileID]" />
      </key>
      <one-to-many class="FFileInteraction"/>
    </bag>
    <bag name="FFileOperationList" inverse="true"  lazy="true">
      <key>
