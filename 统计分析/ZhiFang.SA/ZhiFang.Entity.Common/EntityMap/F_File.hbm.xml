    <property name="WeiXinAuthor" column="[WeiXinAuthor]" type="String" not-null="false" length="100" />
    <property name="WeiXinDigest" column="[WeiXinDigest]" type="String" not-null="false" length="100" />
    <property name="WeiXinUrl" column="[WeiXinUrl]" type="String" not-null="false" length="200" />
    <property name="WeiXinContentsourceurl" column="[WeiXinContent_source_url]" type="String" not-null="false" length="200" />
