    <property name="PublisherDateTime" column="[PublisherDateTime]" type="DateTime" not-null="false" length="8" />
    <property name="IsSyncWeiXin" column="[IsSyncWeiXin]" type="Boolean" not-null="false" length="1" />
    <property name="WeiXinContent" column="[WeiXinContent]" length="10240000" type="String" not-null="false" />
     <property name="ThumbnailsPath" column="[ThumbnailsPath]" type="String" not-null="false" length="100" />
    <property name="ThumbnailsMemo" column="[ThumbnailsMemo]" type="String" not-null="false" length="500" />
    
    <property name="Mediaid" column="[Media_id]" type="String" not-null="false" length="100" />
    <property name="Thumbmediaid" column="[Thumb_media_id]" type="String" not-null="false" length="100" />
