<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.Common" assembly="ZhiFang.Entity.Common">
  <class name="FFile" table="[dbo].[F_File]">
    <id name="Id" column="[FileID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="Title" column="[Title]" type="String" not-null="false" length="100" />
    <property name="No" column="[No]" type="String" not-null="false" length="100" />
    <property name="Content" column="[Content]" type="String" length="10240000" not-null="false" />
    <property name="Type" column="[Type]" type="Int32" not-null="false" length="4" />
    <property name="Status" column="[Status]" type="Int32" not-null="false" length="4" />
    <property name="Keyword" column="[Keyword]" type="String" not-null="false" length="100" />
    <property name="Summary" column="[Summary]" type="String" not-null="false" length="100" />
    <property name="Source" column="[Source]" type="String" not-null="false" length="100" />
    <property name="VersionNo" column="[VersionNo]" type="String" not-null="false" length="100" />
    <property name="Pagination" column="[Pagination]" type="Int32" not-null="false" length="4" />
    <property name="ReviseNo" column="[ReviseNo]" type="String" not-null="false" length="100" />
    <property name="ReviseReason" column="[ReviseReason]" type="String" not-null="false" length="500" />
    <property name="ReviseContent" column="[ReviseContent]" type="String" not-null="false" />
    <property name="ReviseTime" column="[ReviseTime]" type="DateTime" not-null="false" length="8" />
    <property name="BeginTime" column="[BeginTime]" type="DateTime" not-null="false" length="8" />
    <property name="EndTime" column="[EndTime]" type="DateTime" not-null="false" length="8" />
    <property name="Memo" column="[Memo]" type="String" not-null="false" length="500" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="IsUse" column="[IsUse]" type="Int32" not-null="false" length="4" />
    <property name="CreatorName" column="[CreatorName]" type="String" not-null="false" length="50" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <property name="OriginalFileID" column="[OriginalFileID]" type="Int64" not-null="false" length="8" />
    <property name="DrafterId" column="[DrafterId]" type="Int64" not-null="false" length="8" />
    <property name="DrafterCName" column="[DrafterCName]" type="String" not-null="false" length="50" />
    <property name="CheckerId" column="[CheckerId]" type="Int64" not-null="false" length="8" />
    <property name="CheckerName" column="[CheckerName]" type="String" not-null="false" length="50" />
    <property name="ApprovalId" column="[ApprovalId]" type="Int64" not-null="false" length="8" />
    <property name="ApprovalName" column="[ApprovalName]" type="String" not-null="false" length="50" />
    <property name="PublisherId" column="[PublisherId]" type="Int64" not-null="false" length="8" />
    <property name="PublisherName" column="[PublisherName]" type="String" not-null="false" length="50" />
    <property name="IsTop" column="[IsTop]" type="Boolean" not-null="false" length="1" />
    <property name="IsDiscuss" column="[IsDiscuss]" type="Boolean" not-null="false" length="1" />
    <property name="Counts" column="[Counts]" type="Int32" not-null="false" length="5" />

    <property name="DrafterDateTime" column="[DrafterDateTime]" type="DateTime" not-null="false" length="8" />
