    <bag name="FFileReadingLogList" inverse="true"  lazy="true">
      <key>
        <column name="[FileID]" />
      </key>
      <one-to-many class="FFileReadingLog"/>
    </bag>
    <bag name="FFileReadingUserList" inverse="true" lazy="true">
      <key>
        <column name="[FileID]" />
      </key>
      <one-to-many class="FFileReadingUser"/>
    </bag>
  </class>
</hibernate-mapping>
