<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.WeiXin" assembly="ZhiFang.Entity.WeiXin">
	<class name="BAccountType" table="[dbo].[B_AccountType]">
		<id name="Id" column="[AccountTypeID]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="false" length="8" />
		<property name="Name" column="[Name]" type="String" not-null="false" length="40" />
		<property name="EName" column="[EName]" type="String" not-null="false" length="50" />
		<property name="SName" column="[SName]" type="String" not-null="false" length="40" />
		<property name="Shortcode" column="[Shortcode]" type="String" not-null="false" length="20" />
		<property name="PinYinZiTou" column="[PinYinZiTou]" type="String" not-null="false" length="50" />
		<property name="Comment" column="[Comment]" type="String" not-null="false" length="300" />
		<property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<bag name="BWeiXinAccountList" inverse="true" cascade="all-delete-orphan" lazy="true">
			<key>
                <column name="[AccountTypeID]" />
            </key>
			<one-to-many class="BWeiXinAccount"/>
		</bag>
	</class>
</hibernate-mapping>
