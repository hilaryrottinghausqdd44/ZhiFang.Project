		<!--<processModel enable="true"/>-->
	</system.web>

	<system.webServer>
		<handlers>
			<add name="ajaxpro" path="ajaxpro/*.ashx" verb="POST,GET" type="AjaxPro.AjaxHandlerFactory,AjaxPro" />
		</handlers>
