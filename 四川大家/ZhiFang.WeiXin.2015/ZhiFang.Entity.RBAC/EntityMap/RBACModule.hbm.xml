<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.RBAC" assembly="ZhiFang.Entity.RBAC">
	<class name="RBACModule" table="[dbo].[RBAC_Module]">
		<id name="Id" column="[ModuleID]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
		<property name="ParentID" column="[ParentID]" type="Int64" not-null="false" length="8" />
		<property name="LevelNum" column="[LevelNum]" type="Int32" not-null="false" length="4" />
		<property name="TreeCatalog" column="[TreeCatalog]" type="Int32" not-null="false" length="4" />
		<property name="IsLeaf" column="[IsLeaf]" type="Boolean" not-null="false" length="1" />
		<property name="ModuleType" column="[ModuleType]" type="Int32" not-null="false" length="4" />
		<property name="PicFile" column="[PicFile]" type="String" not-null="false" length="200" />
		<property name="Url" column="[URL]" type="String" not-null="false" length="150" />
		<property name="Para" column="[Para]" type="String" not-null="false" length="500" />
		<property name="Owner" column="[Owner]" type="String" not-null="true" length="50" />
		<property name="UseCode" column="[UseCode]" type="String" not-null="false" length="50" />
		<property name="StandCode" column="[StandCode]" type="String" not-null="false" length="50" />
		<property name="DeveCode" column="[DeveCode]" type="String" not-null="false" length="50" />
		<property name="CName" column="[CName]" type="String" not-null="false" length="50" />
		<property name="EName" column="[EName]" type="String" not-null="false" length="50" />
		<property name="SName" column="[SName]" type="String" not-null="false" length="50" />
		<property name="Shortcode" column="[Shortcode]" type="String" not-null="false" length="20" />
		<property name="PinYinZiTou" column="[PinYinZiTou]" type="String" not-null="false" length="50" />
		<property name="Comment" column="[Comment]" type="String" not-null="false" length="16" />
		<property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
		<property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <many-to-one name="BTDAppComponents" class="BTDAppComponents" lazy="false">
      <column name="[AppComID]" not-null="false"/>
    </many-to-one>
		<bag name="RBACEmpOptionsList" inverse="true" cascade="save-update" lazy="true">
			<key>
                <column name="[DefaultModuleID]" />
            </key>
			<one-to-many class="RBACEmpOptions"/>
		</bag>
		<bag name="RBACModuleOperList" inverse="true" cascade="save-update" lazy="true">
			<key>
                <column name="[ModuleID]" />
            </key>
			<one-to-many class="RBACModuleOper"/>
		</bag>
		<bag name="RBACRoleModuleList" inverse="true" cascade="save-update" lazy="true">
			<key>
                <column name="[ModuleID]" />
            </key>
			<one-to-many class="RBACRoleModule"/>
		</bag>
	</class>
</hibernate-mapping>
