      <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
      <property name="Birthday" column="[Birthday]" type="DateTime" not-null="false" length="8" />
      <property name="Email" column="[Email]" type="String" not-null="false" length="50" />
      <property name="MobileTel" column="[MobileTel]" type="String" not-null="false" length="50" />
      <property name="OfficeTel" column="[OfficeTel]" type="Double" not-null="false" length="8" />
      <property name="ExtTel" column="[ExtTel]" type="Double" not-null="false" length="8" />
      <property name="HomeTel" column="[HomeTel]" type="Double" not-null="false" length="8" />
      <property name="Tel" column="[Tel]" type="String" not-null="false" length="50" />
      <property name="Address" column="[Address]" type="String" not-null="false" length="250" />
      <property name="ZipCode" column="[ZipCode]" type="String" not-null="false" length="50" />
      <property name="IsEnabled" column="[IsEnabled]" type="Int32" not-null="true" length="4" />
      <property name="PicFile" column="[PicFile]" type="String" not-null="false" length="200" />
