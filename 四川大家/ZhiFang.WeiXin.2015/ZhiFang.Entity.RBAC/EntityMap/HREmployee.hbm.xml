      </many-to-one>
      <bag name="HRDeptEmpList" inverse="true" cascade="save-update" lazy="true" >
            <key>
                <column name="[EmpID]" />
            </key>
            <one-to-many class="HRDeptEmp" />
        </bag>
      <bag name="HREmpIdentityList" inverse="true" cascade="save-update" lazy="true" >
