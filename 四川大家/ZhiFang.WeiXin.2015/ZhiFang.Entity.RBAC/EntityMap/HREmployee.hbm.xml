      <property name="ManagerID" column="[ManagerID]" type="Int64" not-null="false" />
      <property name="ManagerName" column="[ManagerName]" type="String" not-null="false" length="50" />
      <many-to-one name="HRDept" class="HRDept" lazy="false">
        <column name="[DeptID]" not-null="false"/>
