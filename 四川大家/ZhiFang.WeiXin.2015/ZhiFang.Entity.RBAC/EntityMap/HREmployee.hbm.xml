        <column name="[DegreeID]" not-null="false"/>
      </many-to-one>
      <many-to-one name="BEducationLevel" class="BEducationLevel" lazy="false">
        <column name="[EducationLevelID]" not-null="false"/>
      </many-to-one>
      <many-to-one name="BHealthStatus" class="BHealthStatus" lazy="false">
        <column name="[HealthStatusID]" not-null="false"/>
