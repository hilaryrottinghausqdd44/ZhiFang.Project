      <property name="EduBackground" column="[EduBackground]" type="String" not-null="false" length="200" />
      <property name="Training" column="[Training]" type="String" not-null="false" length="200" />
      <property name="PersonalHomePage" column="[PersonalHomePage]" type="String" not-null="false" length="200" />
      <property name="JobResume" column="[JobResume]" type="String" not-null="false" length="200" />
      <property name="Family" column="[Family]" type="String" not-null="false" length="200" />
      <property name="EntryTime" column="[EntryTime]" type="DateTime" not-null="false" length="8" />
