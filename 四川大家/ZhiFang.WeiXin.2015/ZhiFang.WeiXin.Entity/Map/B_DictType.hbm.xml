<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.WeiXin.Entity" assembly="ZhiFang.WeiXin.Entity">
	<class name="BDictType" table="[dbo].[B_DictType]">
		<id name="Id" column="[DictTypeID]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="false" length="8" />
		<property name="DictTypeCode" column="[DictTypeCode]" type="String" not-null="true" length="100"  />
		<property name="CName" column="[CName]" type="String" not-null="true" length="100" />
		<property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
		<property name="Memo" column="[Memo]" type="String" not-null="false" length="500" />
		<bag name="BDictList" inverse="true" cascade="all-delete-orphan" lazy="true">
			<key>
                <column name="[DictTypeID]" />
            </key>
			<one-to-many class="BDict"/>
		</bag>
	</class>
</hibernate-mapping>
