<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.WeiXin.Entity" assembly="ZhiFang.WeiXin.Entity">
  <class name="ItemColorDict" table="[dbo].[ItemColorDict]">
    <id name="Id" column="[ColorID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="ColorName" column="[ColorName]" type="String" not-null="false" length="15" />
    <property name="ColorValue" column="[ColorValue]" type="String" not-null="false" length="15" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
  </class>
</hibernate-mapping>
