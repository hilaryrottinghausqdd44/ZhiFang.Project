<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.WeiXin.Entity" assembly="ZhiFang.WeiXin.Entity">
	<class name="SickType" table="[dbo].[SickType]">
		<id name="Id" column="[SickTypeNo]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DTimeStampe]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="CName" column="[CName]" type="String" not-null="false" length="20" />
		<property name="ShortCode" column="[ShortCode]" type="String" not-null="false" length="10" />
		<property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
		<property name="HisOrderCode" column="[HisOrderCode]" type="String" not-null="false" length="21" />
		<!--<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />-->
	</class>
</hibernate-mapping>
