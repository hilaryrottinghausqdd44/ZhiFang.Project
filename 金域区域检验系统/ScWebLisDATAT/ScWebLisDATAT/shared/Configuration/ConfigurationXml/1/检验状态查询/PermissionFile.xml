<?xml version="1.0" encoding="GB2312"?>
<Businesses>
  <Business name="检验状态查询">
    <Table TableEName="VIEW_BarCode_Form_Item_Status">
      <DFD>
        <Condition comment="各家看各家的检验状态">
          <Match field="ClientCName" macro="EmployeeName()" comp="equal" />
        </Condition>
        <Condition comment="管理员看全部">
          <Match field="BarCode" const="1" comp="ne" />
        </Condition>
      </DFD>
    </Table>
    <Table TableEName="VIEW_BarCode_Form_Item">
      <DFD>
        <Condition comment="各家查各家的">
          <Match field="ClientCName" macro="EmployeeName()" comp="equal" />
        </Condition>
        <Condition comment="管理人员看全部">
          <Match role="employee" const="311" comp="equal" />
          <Match field="BarCode" const="1" comp="ne" />
        </Condition>
      </DFD>
    </Table>
  </Business>
</Businesses>