			<generator class="assigned" />
		</id>
		<property name="FeedbackTime" column="[FeedbackTime]" type="DateTime" not-null="false" length="8" />
		<property name="FeedbackContent" column="[FeedbackContent]" type="String" not-null="false" length="500" />
		<property name="AnswerTime" column="[AnswerTime]" type="DateTime" not-null="false" length="8" />
		<property name="AnswerContent" column="[AnswerContent]" type="String" not-null="false" length="500" />
		<property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
		<property name="Memo" column="[Memo]" type="String" not-null="false" length="500" />
		<property name="Phone" column="[Phone]" type="String" not-null="false" length="50" />
		<property name="Email" column="[Email]" type="String" not-null="false" length="100" />
		<property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<many-to-one name="Answer" class="ZhiFang.Digitlab.Entity.HREmployee" lazy="false">
			<column name="[AnswerID]" not-null="false"/>
		</many-to-one>
		<many-to-one name="Creator" class="ZhiFang.Digitlab.Entity.HREmployee" lazy="false">
			<column name="[CreatorID]" not-null="false"/>
		</many-to-one>
	</class>
</hibernate-mapping>
