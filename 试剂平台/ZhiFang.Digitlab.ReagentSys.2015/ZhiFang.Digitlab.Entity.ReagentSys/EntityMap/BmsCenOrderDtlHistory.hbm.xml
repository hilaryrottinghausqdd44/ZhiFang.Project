			<generator class="assigned" />
		</id>
		<property name="OrderDtlNo" column="[OrderDtlNo]" type="String" not-null="true" length="50" />
		<property name="OrderDocNo" column="[OrderDocNo]" type="String" not-null="true" length="50" />
		<property name="ProdGoodsNo" column="[ProdGoodsNo]" type="String" not-null="true" length="50" />
		<property name="ProdOrgName" column="[ProdOrgName]" type="String" not-null="false" length="100" />
		<property name="GoodsName" column="[GoodsName]" type="String" not-null="true" length="100" />
		<property name="GoodsUnit" column="[GoodsUnit]" type="String" not-null="false" length="10" />
		<property name="UnitMemo" column="[UnitMemo]" type="String" not-null="false" length="100" />
		<property name="GoodsQty" column="[GoodsQty]" type="Double" not-null="false" length="8" />
