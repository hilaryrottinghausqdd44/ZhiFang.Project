		<property name="IOFlag" column="[IOFlag]" type="Int32" not-null="false" length="4" />
		<property name="ZX1" column="[ZX1]" type="String" not-null="false" length="100" />
		<property name="ZX2" column="[ZX2]" type="String" not-null="false" length="100" />
		<property name="ZX3" column="[ZX3]" type="String" not-null="false" length="100" />
		<property name="Price" column="[Price]" type="Double" not-null="false" length="8" />
		<many-to-one name="BmsCenOrderDocHistory" class="BmsCenOrderDocHistory" lazy="false">
			<column name="[OrderDocID]" not-null="true"/>
		</many-to-one>
		<many-to-one name="Prod" class="CenOrg" lazy="false">
			<column name="[ProdID]" not-null="true"/>
		</many-to-one>
		<many-to-one name="Goods" class="Goods" lazy="false">
			<column name="[GoodsID]" not-null="true"/>
		</many-to-one>
	</class>
</hibernate-mapping>
