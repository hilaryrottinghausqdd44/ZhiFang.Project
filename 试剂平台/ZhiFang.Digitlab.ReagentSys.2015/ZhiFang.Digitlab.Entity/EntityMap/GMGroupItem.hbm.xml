<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
  <class name="GMGroupItem" table="[dbo].[GM_GroupItem]">
    <id name="Id" column="[GroupItemID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <property name="DefaultReportValue" column="[DefaultReportValue]" type="String" not-null="false" length="300" />
    <many-to-one name="GMGroup" class="GMGroup" lazy="false">
      <column name="[GroupID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="ItemAllItem" class="ItemAllItem" lazy="false">
      <column name="[ItemID]" not-null="false"/>
    </many-to-one>
  </class>
</hibernate-mapping>
