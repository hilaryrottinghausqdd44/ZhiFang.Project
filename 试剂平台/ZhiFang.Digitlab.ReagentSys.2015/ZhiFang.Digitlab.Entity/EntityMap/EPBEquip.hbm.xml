<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
  <class name="EPBEquip" table="[dbo].[EP_B_Equip]">
    <id name="Id" column="[EquipID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="UseCode" column="[UseCode]" type="String" not-null="false" length="50" />
    <property name="StandCode" column="[StandCode]" type="String" not-null="false" length="50" />
    <property name="DeveCode" column="[DeveCode]" type="String" not-null="false" length="50" />
    <property name="EquipChannel" column="[EquipChannel]" type="String" not-null="false" length="50" />
    <property name="CName" column="[CName]" type="String" not-null="false" length="50" />
    <property name="EName" column="[EName]" type="String" not-null="false" length="50" />
    <property name="SName" column="[SName]" type="String" not-null="false" length="50" />
    <property name="Shortcode" column="[Shortcode]" type="String" not-null="false" length="20" />
    <property name="PinYinZiTou" column="[PinYinZiTou]" type="String" not-null="false" length="50" />
    <property name="Comment" column="[Comment]" type="String" not-null="false" length="16" />
    <property name="GroupID" column="[GroupID]" type="Int64" not-null="false" length="8" />
    <property name="Computer" column="[Computer]" type="String" not-null="false" length="20" />
    <property name="ComProgram" column="[ComProgram]" type="String" not-null="false" length="20" />
    <property name="DoubleDir" column="[DoubleDir]" type="Int32" not-null="false" length="4" />
    <!--<property name="EquipType" column="[EquipType]" type="Int32" not-null="false" length="4" />-->
    <property name="LicenceKey" column="[LicenceKey]" type="String" not-null="false" length="30" />
    <property name="LicenceType" column="[LicenceType]" type="String" not-null="false" length="25" />
    <property name="SQH" column="[SQH]" type="String" not-null="false" length="4" />
    <property name="SNo" column="[SNo]" type="Int32" not-null="false" length="4" />
    <property name="LicenceDate" column="[LicenceDate]" type="DateTime" not-null="false" length="8" />
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <property name="EquipResultType" column="[EquipResultType]" type="Int32" not-null="false" length="4" />
    <bag name="EPModuleList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[EquipID]" />
      </key>
      <one-to-many class="EPModule"/>
    </bag>
    <bag name="GMGroupEquipList" inverse="true" lazy="true">
      <key>
        <column name="[EquipID]" />
      </key>
      <one-to-many class="GMGroupEquip"/>
    </bag>
    <bag name="EPEquipItemList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[EquipID]" />
      </key>
      <one-to-many class="EPEquipItem"/>
    </bag>
    <many-to-one name="EPEquipType" class="EPEquipType" lazy="false">
      <column name="[EquipTypeID]" not-null="false"/>
    </many-to-one>
  </class>
</hibernate-mapping>
