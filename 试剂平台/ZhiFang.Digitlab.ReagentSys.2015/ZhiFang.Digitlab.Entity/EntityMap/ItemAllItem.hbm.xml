<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
  <class name="ItemAllItem" table="[dbo].[Item_AllItem]">
    <id name="Id" column="[ItemID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="CName" column="[CName]" type="String" not-null="false" length="100" />
    <property name="EName" column="[EName]" type="String" not-null="false" length="100" />
    <property name="SName" column="[SName]" type="String" not-null="false" length="100" />
    <property name="ItemType" column="[ItemType]" type="Int32" not-null="false" length="4" />
    <property name="Unit" column="[Unit]" type="String" not-null="false" length="50" />
    <property name="RefRange" column="[RefRange]" type="String" not-null="false" length="500" />
    <property name="ValueType" column="[ValueType]" type="Int32" not-null="false" length="4" />
    <property name="SamplingRequire" column="[SamplingRequire]" type="String" not-null="false" length="8000" />
    <property name="ClinicalSignificance" column="[ClinicalSignificance]" type="String" not-null="false" length="8000" />
    <property name="ChargeType" column="[ChargeType]" type="Int32" not-null="false" length="4" />
    <property name="ItemCharge" column="[ItemCharge]" type="Decimal" not-null="false" length="9" />
    <property name="Comment" column="[Comment]" type="String" not-null="false" length="8000" />
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="UseCode" column="[UseCode]" type="String" not-null="false" length="50" />
    <property name="StandCode" column="[StandCode]" type="String" not-null="false" length="100" />
    <property name="DeveCode" column="[DeveCode]" type="String" not-null="false" length="50" />
    <property name="Shortcode" column="[Shortcode]" type="String" not-null="false" length="100" />
    <property name="PinYinZiTou" column="[PinYinZiTou]" type="String" not-null="false" length="50" />
    <property name="Precision" column="[Precision]" type="Int32" not-null="false" length="4" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <property name="IsHisOrder" column="[IsHisOrder]" type="Boolean" not-null="false" length="1" />
    <many-to-one name="BSpecialty" class="BSpecialty" >
      <column name="[SpecialtyID]" not-null="false"/>
    </many-to-one>
    
    <bag name="GMGroupItemList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[ItemID]" />
      </key>
      <one-to-many class="GMGroupItem"/>
    </bag>
    <bag name="EPEquipItemList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[ItemID]" />
      </key>
      <one-to-many class="EPEquipItem"/>
    </bag>
    
   
  </class>
</hibernate-mapping>
