    <many-to-one name="BmsCenSaleDoc" class="BmsCenSaleDoc" lazy="false">
      <column name="[SaleDocID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="Lab" class="CenOrg" lazy="false">
