<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity.ReagentSys" assembly="ZhiFang.Digitlab.Entity.ReagentSys">
  <class name="BmsAccountInput" table="[dbo].[BmsAccountInput]">
    <id name="Id" column="[AccountID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabName" column="[LabName]" type="String" not-null="false" length="100" />
    <property name="CName" column="[CName]" type="String" not-null="false" length="100" />
    <property name="EName" column="[EName]" type="String" not-null="false" length="100" />
    <property name="ShortCode" column="[ShortCode]" type="String" not-null="false" length="100" />
    <property name="Comment" column="[Comment]" type="String" not-null="false" length="1000" />
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <property name="TotalPrice" column="[TotalPrice]" type="Double" not-null="false" length="8" />
    <property name="UserID" column="[UserID]" type="Int64" not-null="false" length="8" />
    <property name="UserName" column="[UserName]" type="String" not-null="false" length="50" />
    <many-to-one name="Lab" class="CenOrg" lazy="false">
      <column name="[LabID]" not-null="false"/>
    </many-to-one>
    <bag name="BmsAccountSaleDocList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[AccountID]" />
      </key>
      <one-to-many class="BmsAccountSaleDoc"/>
    </bag>
  </class>
</hibernate-mapping>
