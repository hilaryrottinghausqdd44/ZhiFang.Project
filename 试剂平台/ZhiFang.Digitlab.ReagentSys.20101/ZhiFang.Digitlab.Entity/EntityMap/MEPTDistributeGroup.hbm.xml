<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
	<class name="MEPTDistributeGroup" table="[dbo].[MEPT_DistributeGroup]">
		<id name="Id" column="[DistributeGroupID]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
		<property name="Name" column="[Name]" type="String" not-null="false" length="40" />
		<property name="SName" column="[SName]" type="String" not-null="false" length="40" />
		<property name="Shortcode" column="[Shortcode]" type="String" not-null="false" length="20" />
		<property name="PinYinZiTou" column="[PinYinZiTou]" type="String" not-null="false" length="50" />
		<property name="Comment" column="[Comment]" type="String" not-null="false" length="16" />
		<property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
		<property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
		<many-to-one name="EPBEquip" class="EPBEquip" lazy="false">
			<column name="[EquipID]" not-null="false"/>
		</many-to-one>
		<many-to-one name="GMGroup" class="GMGroup" lazy="false">
			<column name="[GroupID]" not-null="false"/>
		</many-to-one>
		<bag name="MEPTDistributeGroupItemList" inverse="true" cascade="all-delete-orphan" lazy="true">
			<key>
                <column name="[DistributeGroupID]" />
            </key>
			<one-to-many class="MEPTDistributeGroupItem"/>
		</bag>
		<bag name="MEPTDistributeRuleDetailList" inverse="true" cascade="all-delete-orphan" lazy="true">
			<key>
                <column name="[DistributeGroupID]" />
            </key>
			<one-to-many class="MEPTDistributeRuleDetail"/>
		</bag>
		<bag name="MEPTSpecialDistributeRuleList" inverse="true" cascade="all-delete-orphan" lazy="true">
			<key>
                <column name="[DistributeGroupID]" />
            </key>
			<one-to-many class="MEPTSpecialDistributeRule"/>
		</bag>
	</class>
</hibernate-mapping>
