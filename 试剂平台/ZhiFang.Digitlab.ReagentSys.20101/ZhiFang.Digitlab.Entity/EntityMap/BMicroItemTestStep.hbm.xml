<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
  <class name="BMicroItemTestStep" table="[dbo].[B_MicroItemTestStep]">
    <id name="Id" column="[MicroItemTestStepID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="IsNextStep" column="[IsNextStep]" type="Boolean" not-null="false" length="1" />
    <property name="Comment" column="[Comment]" type="String" not-null="false" length="16" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <property name="IsDefault" column="[IsDefault]" type="Boolean" not-null="false" length="1" />
    <many-to-one name="BMicroItemTestStepGroup" class="BMicroItemTestStepGroup" lazy="false">
      <column name="[MicroItemTestStepGroupID]" not-null="true"/>
    </many-to-one>
    <many-to-one name="BMicroTestStep" class="BMicroTestStep" lazy="false">
      <column name="[MicroTestStepID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="Next" class="BMicroTestStep" lazy="false">
      <column name="[NextMicroTestStepID]" not-null="false"/>
    </many-to-one>
    <bag name="EPMicroEquipItemStepList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[MicroItemTestStepID]" />
      </key>
      <one-to-many class="EPMicroEquipItemStep"/>
    </bag>
  </class>
</hibernate-mapping>
