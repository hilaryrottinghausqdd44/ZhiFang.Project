<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
	<class name="MEEquipMicroStepInfo" table="[dbo].[ME_EquipMicroStepInfo]">
		<id name="Id" column="[EquipMicroStepValueID]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
		<property name="MicroChannel" column="[MicroChannel]" type="String" not-null="false" length="50" />
		<property name="ReportValue" column="[ReportValue]" type="String" not-null="false" length="300" />
		<property name="ResultLink" column="[ResultLink]" type="String" not-null="false" length="100" />
		<property name="ResultComment" column="[ResultComment]" type="String" not-null="false" length="16" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
		<many-to-one name="BMicro" class="BMicro" >
			<column name="[MicroID]" not-null="false"/>
		</many-to-one>
		<many-to-one name="MEEquipSampleForm" class="MEEquipSampleForm" >
			<column name="[EquipSampleFormID]" not-null="false"/>
		</many-to-one>
		<many-to-one name="MEEquipSampleItem" class="MEEquipSampleItem" >
			<column name="[EquipResultID]" not-null="false"/>
		</many-to-one>
		<bag name="MEEquipMicroSuscResultList" inverse="true" cascade="all-delete-orphan" lazy="true">
			<key>
                <column name="[EquipMicroStepValueID]" />
            </key>
			<one-to-many class="MEEquipMicroSuscResult"/>
		</bag>
	</class>
</hibernate-mapping>
