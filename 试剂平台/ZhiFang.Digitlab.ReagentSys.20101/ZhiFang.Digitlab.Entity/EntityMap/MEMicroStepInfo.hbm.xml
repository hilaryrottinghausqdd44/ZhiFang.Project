<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
	<class name="MEMicroStepInfo" table="[dbo].[ME_MicroStepInfo]">
		<id name="Id" column="[MicroStepinfoValueID]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
		<property name="EquipMicroStepValueID" column="[EquipMicroStepValueID]" type="Int64" not-null="false" length="8" />
		<property name="ReportValue" column="[ReportValue]" type="String" not-null="false" length="300" />
		<property name="AlarmLevel" column="[AlarmLevel]" type="Int32" not-null="false" length="4" />
		<property name="ResultLink" column="[ResultLink]" type="String" not-null="false" length="100" />
		<property name="ResultComment" column="[ResultComment]" type="String" not-null="false" length="16" />
		<property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
		<property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
		<many-to-one name="BMicroTestItemInfo" class="BMicroTestItemInfo" >
			<column name="[MicroTestItemInfoID]" not-null="false"/>
		</many-to-one>
		<many-to-one name="MEGroupSampleItem" class="MEGroupSampleItem" >
			<column name="[ResultID]" not-null="false"/>
		</many-to-one>
	</class>
</hibernate-mapping>
