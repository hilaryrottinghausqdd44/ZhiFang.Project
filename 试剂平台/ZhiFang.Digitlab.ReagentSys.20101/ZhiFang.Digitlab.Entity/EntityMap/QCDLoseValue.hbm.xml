			<column name="[QCDataID]" not-null="false"/>
		</many-to-one>
    <many-to-one name="LoseOperator" class="HREmployee" lazy="false" >
      <column name="[LoseOperatorID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="LoseExaminer" class="HREmployee" lazy="false" >
      <column name="[LoseExaminerID]" not-null="false"/>
    </many-to-one>
	</class>
</hibernate-mapping>
