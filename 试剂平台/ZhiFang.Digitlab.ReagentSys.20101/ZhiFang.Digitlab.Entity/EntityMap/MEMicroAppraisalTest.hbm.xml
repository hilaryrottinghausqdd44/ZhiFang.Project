<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
	<class name="MEMicroAppraisalTest" table="[dbo].[ME_MicroAppraisalTest]">
		<id name="Id" column="[AppraisalTestID]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
		<property name="GroupSampleFormID" column="[GroupSampleFormID]" type="Int64" not-null="false" length="8" />
		<property name="GSampleNo" column="[GSampleNo]" type="String" not-null="false" length="20" />
		<property name="TestID" column="[TestID]" type="Int64" not-null="false" length="8" />
		<property name="TestValueID" column="[TestValueID]" type="Int64" not-null="false" length="8" />
		<property name="Memo" column="[Memo]" type="String" not-null="false" length="500" />
		<property name="IsReport" column="[isReport]" type="Boolean" not-null="false" length="1" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<property name="EmpID" column="[EmpID]" type="Int64" not-null="false" length="8" />
		<property name="EmpName" column="[EmpName]" type="String" not-null="false" length="50" />
		<property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
		<many-to-one name="MEMicroAppraisalValue" class="MEMicroAppraisalValue" >
			<column name="[AppraisalValueID]" not-null="false"/>
		</many-to-one>
	</class>
</hibernate-mapping>
