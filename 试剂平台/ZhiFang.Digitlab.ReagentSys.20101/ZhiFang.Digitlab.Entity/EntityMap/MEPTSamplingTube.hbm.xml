<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
  <class name="MEPTSamplingTube" table="[dbo].[MEPT_SamplingTube]">
    <id name="Id" column="[SamplingTubeID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="CName" column="[CName]" type="String" not-null="false" length="50" />
    <property name="EName" column="[EName]" type="String" not-null="false" length="50" />
    <property name="SName" column="[SName]" type="String" not-null="false" length="50" />
    <property name="Shortcode" column="[Shortcode]" type="String" not-null="false" length="20" />
    <property name="ColorName" column="[ColorName]" type="String" not-null="false" length="50" />
    <property name="ColorValue" column="[ColorValue]" type="String" not-null="false" length="50" />
    <property name="Capacity" column="[Capacity]" type="Decimal" not-null="false" length="5" />
    <property name="Unit" column="[Unit]" type="String" not-null="false" length="50" />
    <property name="MinCapacity" column="[MinCapacity]" type="Decimal" not-null="false" length="5" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="PinYinZiTou" column="[PinYinZiTou]" type="String" not-null="false" length="50" />
    <property name="Comment" column="[Comment]" type="String" not-null="false" length="16" />
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <many-to-one name="MEPTBCharge" class="MEPTBCharge" lazy="false">
      <column name="[ChargeID]" not-null="false"/>
    </many-to-one>
    <bag name="MEPTSamplingGroupList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[SamplingTubeID]" />
      </key>
      <one-to-many class="MEPTSamplingGroup"/>
    </bag>
  </class>
</hibernate-mapping>
