<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
    <class name="HREmpIdentity" table="[dbo].[HR_EmpIdentity]" >
        <id name="Id" column="[EmpIdenID]" type="Int64" >
            <generator class="assigned" />
        </id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
        <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" /> 
        <property name="TSysID" column="[TSysID]" type="Int64" not-null="false" /> 
        <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" /> 
        <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" /> 
        <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" /> 
        <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" /> 
        <many-to-one name="HREmployee" class="HREmployee" not-null="false" lazy="false">
            <column name="[EmpID]" />
        </many-to-one>
      <many-to-one name="STypeDetail" class="STypeDetail" lazy="false">
        <column name="[TSysID]" not-null="false"/>
      </many-to-one>
    </class>
</hibernate-mapping>

