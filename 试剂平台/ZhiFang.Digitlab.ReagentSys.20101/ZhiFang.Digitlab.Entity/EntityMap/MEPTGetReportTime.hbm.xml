<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
  <class name="MEPTGetReportTime" table="[dbo].[MEPT_GetReportTime]">
    <id name="Id" column="[GetReportTimeID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="CName" column="[CName]" type="String" not-null="false" length="50" />
    <property name="SName" column="[SName]" type="String" not-null="false" length="50" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="Comment" column="[Comment]" type="String" not-null="false" length="16" />
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="PinYinZiTou" column="[PinYinZiTou]" type="String" not-null="false" length="50" />
    <property name="TimeComment" column="[TimeComment]" type="String" not-null="false" length="500" />
    <property name="AllTimeComment" column="[AllTimeComment]" type="String" not-null="false" length="500" />
    <property name="NeedTime" column="[NeedTime]" type="Int32" not-null="false" length="4" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <!--<one-to-one name="MEPTBSpecialTimeType" lazy="false" />-->
    <many-to-one name="MEPTBSpecialTimeType" class="MEPTBSpecialTimeType" lazy="false">
      <column name="[SpecialTimeTypeID]" not-null="false"/>
    </many-to-one>
    <bag name="MEPTGetReportTimeOfItemList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[GetReportTimeID]" />
      </key>
      <one-to-many class="MEPTGetReportTimeOfItem"/>
    </bag>
    <bag name="MEPTGetReportTimeOfSickTypeList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[GetReportTimeID]" />
      </key>
      <one-to-many class="MEPTGetReportTimeOfSickType"/>
    </bag>
    <bag name="MEPTSampleItemList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[GetReportTimeID]" />
      </key>
      <one-to-many class="MEPTSampleItem"/>
    </bag>
  </class>
</hibernate-mapping>
