    <id name="Id" column="[QCDataID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="ReceiveTime" column="[ReceiveTime]" type="DateTime" not-null="true" length="8" />
    <property name="QCDataLotNo" column="[QCDataLotNo]" type="Int32" not-null="true" length="4" />
    <property name="OriglValue" column="[OriglValue]" type="String" not-null="false" length="40" />
    <property name="ReportValue" column="[ReportValue]" type="String" not-null="false" length="40" />
    <property name="QuanValue" column="[QuanValue]" type="Double" not-null="false" length="8" />
    <property name="QuanValue2" column="[QuanValue2]" type="Double" not-null="false" length="8" />
    <property name="CVValue" column="[CVValue]" type="String" not-null="false" length="20" />
    <property name="ResultStatus" column="[ResultStatus]" type="Int32" not-null="false" length="4" />
    <property name="IsControl" column="[IsControl]" type="Int32" not-null="false" length="4" />
    <property name="QCControlInfo" column="[QCControlInfo]" type="String" not-null="false" length="50" />
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="IsEquipResult" column="[IsEquipResult]" type="Boolean" not-null="false" length="1" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <property name="QCComment" column="[QCComment]" type="String" not-null="false" length="200" />
    <many-to-one name="Operator" class="HREmployee" lazy="false">
      <column name="[OperatorID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="MEEquipSampleItem" class="MEEquipSampleItem" lazy="false">
      <column name="[EquipResultID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="QCItem" class="QCItem" lazy="false">
      <column name="[QCItemID]" not-null="true"/>
    </many-to-one>
    <!--<bag name="QCDLoseValueList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[QCDataID]" />
      </key>
      <one-to-many class="QCDLoseValue"/>
    </bag>-->
    <one-to-one name="QCDLoseValue" class="QCDLoseValue" cascade="all-delete-orphan" property-ref="QCDValue"></one-to-one>
  </class>
</hibernate-mapping>
