		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<property name="ICO" column="[ICO]" type="String" not-null="false" length="1073741823" />
		<bag name="BSoftWareVersionManagerList" inverse="true" lazy="true">
			<key>
                <column name="[Code]" />
            </key>
			<one-to-many class="BSoftWareVersionManager"/>
		</bag>
	</class>
</hibernate-mapping>
