<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
	<class name="BTDAppComponentsOperate" table="[dbo].[BT_D_AppComponentsOperate]">
		<id name="Id" column="[AppComOperateID]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
		<property name="AppComOperateKeyWord" column="[AppComOperateKeyWord]" type="String" not-null="false" length="50" />
		<property name="AppComOperateName" column="[AppComOperateName]" type="String" not-null="false" length="50" />
    <property name="RowFilterBase" column="[RowFilterBase]" type="String" not-null="false" length="500" />
		<property name="Creator" column="[Creator]" type="String" not-null="false" length="50" />
		<property name="Modifier" column="[Modifier]" type="String" not-null="false" length="50" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
		<many-to-one name="BTDAppComponents" class="BTDAppComponents" lazy="false">
			<column name="[AppComID]" not-null="false"/>
		</many-to-one>
		<bag name="RBACModuleOperList" inverse="true" cascade="save-update" lazy="true">
			<key>
                <column name="[AppComOperateID]" />
            </key>
			<one-to-many class="RBACModuleOper"/>
		</bag>
	</class>
</hibernate-mapping>
