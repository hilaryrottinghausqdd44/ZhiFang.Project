<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
	<class name="BPatientInfo" table="[dbo].[B_PatientInfo]">
		<id name="Id" column="[PatientKeyID]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
		<property name="PatientID" column="[PatientID]" type="String" not-null="true" length="20" />
		<property name="PatNo" column="[PatNo]" type="String" not-null="false" length="20" />
		<property name="CName" column="[CName]" type="String" not-null="false" length="40" />
		<property name="Birthday" column="[Birthday]" type="DateTime" not-null="false" length="8" />
		<property name="Telephone" column="[Telephone]" type="String" not-null="false" length="30" />
		<property name="Address" column="[Address]" type="String" not-null="false" length="200" />
		<property name="ZipCode" column="[ZipCode]" type="String" not-null="false" length="10" />
		<property name="CertificateNo" column="[CertificateNo]" type="Int32" not-null="false" length="4" />
		<property name="CertificateNumber" column="[CertificateNumber]" type="String" not-null="false" length="50" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
		<many-to-one name="BNationality" class="BNationality" lazy="false">
			<column name="[NationalityID]" not-null="false"/>
		</many-to-one>
		<many-to-one name="BSex" class="BSex" lazy="false">
			<column name="[SexID]" not-null="false"/>
		</many-to-one>
		<bag name="MEPTOrderFormList" inverse="true" cascade="all-delete-orphan" lazy="true">
			<key>
                <column name="[PatientKeyID]" />
            </key>
			<one-to-many class="MEPTOrderForm"/>
		</bag>
	</class>
</hibernate-mapping>
