<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
  <class name="MEPTSampleDelivery" table="[dbo].[MEPT_SampleDelivery]">
    <id name="Id" column="[SampleDeliveryID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="SampleDeliveryNo" column="[SampleDeliveryNo]" type="String" not-null="true" length="30" />
    <property name="SampleDeliveryDate" column="[SampleDeliveryDate]" type="DateTime" not-null="true" length="8" />
    <property name="SampleDeliveryMan" column="[SampleDeliveryMan]" type="String" not-null="true" length="30" />
    <property name="PrintCount" column="[PrintCount]" type="Int32" not-null="false" length="4" />
    <property name="PrintTime" column="[PrintTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <many-to-one name="DeliveryLab" class="BLaboratory" lazy="false">
      <column name="[DeliveryLabID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="DeliveryDept" class="HRDept" lazy="false">
      <column name="[DeliveryDeptID]" not-null="false"/>
    </many-to-one>
    <bag name="MEPTSampleDeliveryConditonList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[SampleDeliveryID]" />
      </key>
      <one-to-many class="MEPTSampleDeliveryConditon"/>
    </bag>
  </class>
</hibernate-mapping>
