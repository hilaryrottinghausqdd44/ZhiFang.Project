<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
	<class name="QCRulesCon" table="[dbo].[QC_RulesCon]">
		<id name="Id" column="[QCRuleConID]" type="Int64">
			<generator class="assigned" />
		</id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
		<property name="JudgeIndex" column="[JudgeIndex]" type="Int32" not-null="false" length="4" />
		<property name="LoseType" column="[LoseType]" type="Int32" not-null="false" length="4" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
		<many-to-one name="QCRuleBase" class="QCRuleBase" lazy="false">
			<column name="[QCRuleBaseID]" not-null="false"/>
		</many-to-one>
		<many-to-one name="QCRuleUse" class="QCRuleUse" lazy="false">
			<column name="[QCRuleUseID]" not-null="false"/>
		</many-to-one>
	</class>
</hibernate-mapping>
