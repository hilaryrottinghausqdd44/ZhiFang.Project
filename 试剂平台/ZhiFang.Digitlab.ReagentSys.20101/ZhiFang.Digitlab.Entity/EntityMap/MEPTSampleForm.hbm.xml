<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
  <class name="MEPTSampleForm" table="[dbo].[MEPT_SampleForm]">
    <id name="Id" column="[SampleFormID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="false" length="8" />
    <property name="BarCode" column="[BarCode]" type="String" not-null="false" length="30" />
    <property name="IsSpiltItem" column="[IsSpiltItem]" type="Int32" not-null="false" length="4" />
    <property name="IsSendOut" column="[IsSendOut]" type="Int32" not-null="false" length="4" />
    <property name="SendOutLabID" column="[SendOutLabID]" type="Int64" not-null="false" length="8" />
    <property name="SampleCap" column="[SampleCap]" type="Double" not-null="false" length="8" />
    <property name="SampleCount" column="[SampleCount]" type="Int32" not-null="false" length="4" />
    <property name="CollectPart" column="[CollectPart]" type="String" not-null="false" length="200" />
    <property name="BarCodeOpTime" column="[BarCodeOpTime]" type="DateTime" not-null="false" length="8" />
    <property name="SerialScanTime" column="[SerialScanTime]" type="String" not-null="false" length="30" />
    <property name="BarCodeSource" column="[BarCodeSource]" type="Int32" not-null="false" length="4" />
    <property name="PrintCount" column="[PrintCount]" type="Int32" not-null="false" length="4" />
    <property name="ZDY1" column="[ZDY1]" type="String" not-null="false" length="200" />
    <property name="ZDY2" column="[ZDY2]" type="String" not-null="false" length="200" />
    <property name="ZDY3" column="[ZDY3]" type="String" not-null="false" length="200" />
    <property name="ZDY4" column="[ZDY4]" type="String" not-null="false" length="200" />
    <property name="ZDY5" column="[ZDY5]" type="String" not-null="false" length="200" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <property name="Amount" column="[Amount]" type="String" not-null="false" length="50" />
    <property name="SampleCharacterName" column="[SampleCharacterName]" type="String" not-null="false" length="50" />
    <property name="SampleCharacterID" column="[SampleCharacterID]" type="Int64" not-null="false" length="8" />
    <many-to-one name="BSampleStatus" class="BSampleStatus" >
      <column name="[SampleStatusID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="MEPTOrderForm" class="MEPTOrderForm" >
      <column name="[OrderFormID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="BSampleType" class="BSampleType" >
      <column name="[SampleTypeID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="MEPTSamplingGroup" class="MEPTSamplingGroup" >
      <column name="[SamplingGroupID]" not-null="false"/>
    </many-to-one>
    <bag name="MEGroupSampleFormList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[SampleFormID]" />
      </key>
      <one-to-many class="MEGroupSampleForm"/>
    </bag>
    <bag name="MEMicroInoculantList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[SampleFormID]" />
      </key>
      <one-to-many class="MEMicroInoculant"/>
    </bag>
    <bag name="MEPTSampleDeliveryConditonList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[SampleFormID]" />
      </key>
      <one-to-many class="MEPTSampleDeliveryConditon"/>
    </bag>
    <bag name="MEPTSampleInceptConditonList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[SampleFormID]" />
      </key>
      <one-to-many class="MEPTSampleInceptConditon"/>
    </bag>
    <bag name="MEPTSampleItemList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[SampleFormID]" />
      </key>
      <one-to-many class="MEPTSampleItem"/>
    </bag>
    <bag name="MEPTSampleSendConditonList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[SampleFormID]" />
      </key>
      <one-to-many class="MEPTSampleSendConditon"/>
    </bag>
  </class>
</hibernate-mapping>
