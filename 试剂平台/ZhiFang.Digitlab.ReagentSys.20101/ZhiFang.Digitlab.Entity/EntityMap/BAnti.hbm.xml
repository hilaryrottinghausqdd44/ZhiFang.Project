<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
	<class name="BAnti" table="[dbo].[B_Anti]">
		<id name="Id" column="[AntiID]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
		<property name="UseCode" column="[UseCode]" type="String" not-null="false" length="50" />
		<property name="StandCode" column="[StandCode]" type="String" not-null="false" length="50" />
		<property name="DeveCode" column="[DeveCode]" type="String" not-null="false" length="50" />
		<property name="CName" column="[CName]" type="String" not-null="false" length="50" />
		<property name="EName" column="[EName]" type="String" not-null="false" length="50" />
		<property name="SName" column="[SName]" type="String" not-null="false" length="50" />
		<property name="Shortcode" column="[Shortcode]" type="String" not-null="false" length="20" />
		<property name="Concentration" column="[Concentration]" type="String" not-null="false" length="50" />
		<property name="PinYinZiTou" column="[PinYinZiTou]" type="String" not-null="false" length="50" />
		<property name="Comment" column="[Comment]" type="String" not-null="false" length="16" />
		<property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
		<property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
		<bag name="BAntiClassList" inverse="true" cascade="all-delete-orphan" lazy="true">
			<key>
                <column name="[AntiID]" />
            </key>
			<one-to-many class="BAntiClass"/>
		</bag>
		<bag name="BAntiUseList" inverse="true" cascade="all-delete-orphan" lazy="true">
			<key>
                <column name="[AntiID]" />
            </key>
			<one-to-many class="BAntiUse"/>
		</bag>
		<bag name="BMicroAntiRangeList" inverse="true" cascade="all-delete-orphan" lazy="true">
			<key>
                <column name="[AntiID]" />
            </key>
			<one-to-many class="BMicroAntiRange"/>
		</bag>
		<bag name="BMicroGroupAntiClassList" inverse="true" cascade="all-delete-orphan" lazy="true">
			<key>
                <column name="[AntiID]" />
            </key>
			<one-to-many class="BMicroGroupAntiClass"/>
		</bag>
		<bag name="MEMicroDSTValueList" inverse="true"  lazy="true">
			<key>
                <column name="[AntiID]" />
            </key>
			<one-to-many class="MEMicroDSTValue"/>
		</bag>
	</class>
</hibernate-mapping>
