<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
  <class name="QCMatTime" table="[dbo].[QC_MatTime]">
    <id name="Id" column="[QCMatTimeID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="BeginDate" column="[BeginDate]" type="DateTime" not-null="true" length="8" />
    <property name="EndDate" column="[EndDate]" type="DateTime" not-null="false" length="8" />
    <property name="LotNo" column="[LotNo]" type="String" not-null="false" length="20" />
    <property name="CanUseBeginDate" column="[CanUseBeginDate]" type="DateTime" not-null="false" length="8" />
    <property name="CanUseEndDate" column="[CanUseEndDate]" type="DateTime" not-null="false" length="8" />
    <property name="CanUseDateInfo" column="[CanUseDateInfo]" type="String" not-null="false" length="50" />
    <property name="QCMatTimeDesc" column="[QCMatTimeDesc]" type="String" not-null="false" length="200" />
    <property name="CalibratorName" column="[CalibratorName]" type="String" not-null="false" length="40" />
    <property name="CalibratorLotNo" column="[CalibratorLotNo]" type="String" not-null="false" length="40" />
    <property name="CalibratorF" column="[CalibratorF]" type="String" not-null="false" length="40" />    
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <many-to-one name="QCMat" class="QCMat" lazy="false">
      <column name="[QCMatID]" not-null="false"/>
    </many-to-one>
    <bag name="QCItemTimeList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[QCMatTimeID]" />
      </key>
      <one-to-many class="QCItemTime"/>
    </bag>
  </class>
</hibernate-mapping>
