<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
  <class name="OtherStorageItem" table="[dbo].[Other_StorageItem]">
    <id name="Id" column="[StorageItemID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="ProductCode" column="[ProductCode]" type="String" not-null="false" length="40" />
    <property name="ProductName" column="[ProductName]" type="String" not-null="false" length="200" />
    <property name="Specification" column="[Specification]" type="String" not-null="false" length="200" />
    <property name="Unit" column="[Unit]" type="String" not-null="false" length="40" />
    <property name="Amount" column="[Amount]" type="Int32" not-null="false" length="4" />
    <property name="Price" column="[Price]" type="Decimal" not-null="false" length="8" />
    <property name="SumPrice" column="[SumPrice]" type="Decimal" not-null="false" length="8" />
    <property name="ProduceTime" column="[ProduceTime]" type="DateTime" not-null="false" length="8" />
    <property name="BatchCode" column="[BatchCode]" type="String" not-null="false" length="40" />
    <property name="Comment" column="[Comment]" type="String" not-null="false" length="16" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <many-to-one name="BProduce" class="BProduce" lazy="false">
      <column name="[ProduceID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="OtherStorageSingle" class="OtherStorageSingle" lazy="false">
      <column name="[StorageSingleID]" not-null="false"/>
    </many-to-one>
  </class>
</hibernate-mapping>
