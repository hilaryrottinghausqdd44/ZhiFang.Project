<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
  <class name="MEImmunePlate" table="[dbo].[ME_ImmunePlate]">
    <id name="Id" column="[PlateID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="PlateName" column="[PlateName]" type="String" not-null="false" length="40" />
    <property name="PlateComment" column="[PlateComment]" type="String" not-null="false" length="60" />
    <property name="PlateState" column="[PlateState]" type="Int32" not-null="false" length="4" />
    <property name="PStateUpdateTime" column="[PStateUpdateTime]" type="DateTime" not-null="false" length="8" />
    <property name="EReceiveTime" column="[EReceiveTime]" type="DateTime" not-null="false" length="8" />
    <property name="EHasReportResult" column="[EHasReportResult]" type="Int32" not-null="false" length="4" />
    <property name="PlateUseKValue" column="[PlateUseKValue]" type="Int32" not-null="false" length="4" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <many-to-one name="EPBEquip" class="EPBEquip" >
      <column name="[EquipID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="MEEquipSampleForm" class="MEEquipSampleForm" >
      <column name="[EquipSampleFormID]" not-null="false"/>
    </many-to-one>
    <bag name="MEImmuneResultsList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[PlateID]" />
      </key>
      <one-to-many class="MEImmuneResults"/>
    </bag>
  </class>
</hibernate-mapping>
