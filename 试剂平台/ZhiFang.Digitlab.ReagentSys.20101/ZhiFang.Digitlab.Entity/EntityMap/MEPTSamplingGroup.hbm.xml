<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
  <class name="MEPTSamplingGroup" table="[dbo].[MEPT_SamplingGroup]">
    <id name="Id" column="[SamplingGroupID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="CName" column="[CName]" type="String" not-null="false" length="50" />
    <property name="EName" column="[EName]" type="String" not-null="false" length="50" />
    <property name="SName" column="[SName]" type="String" not-null="false" length="50" />
    <property name="Shortcode" column="[Shortcode]" type="String" not-null="false" length="20" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="Comment" column="[Comment]" type="String" not-null="false" length="16" />
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="Destination" column="[Destination]" type="String" not-null="false" length="50" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <many-to-one name="MEPTSamplingGroupPrint" class="MEPTSamplingGroupPrint" lazy="false">
      <column name="[SamplingGroupPrintID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="BSpecialty" class="BSpecialty" lazy="false">
      <column name="[SpecialtyID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="MEPTSamplingTube" class="MEPTSamplingTube" lazy="false">
      <column name="[SamplingTubeID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="BSampleType" class="BSampleType" lazy="false">
      <column name="[SampleTypeID]" not-null="false"/>
    </many-to-one>
    <bag name="MEPTItemSplitList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[SamplingGroupID]" />
      </key>
      <one-to-many class="MEPTItemSplit"/>
    </bag>
    <bag name="MEPTSamplingItemList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[SamplingGroupID]" />
      </key>
      <one-to-many class="MEPTSamplingItem"/>
    </bag>
  </class>
</hibernate-mapping>
