<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
	<class name="MEEquipmentSuppliesRegistrationform" table="[dbo].[ME_EquipmentSuppliesRegistrationform]">
		<id name="Id" column="[ESRFId]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
		<property name="ESRFSerialNo" column="[ESRFSerialNo]" type="String" not-null="false" length="30" />
		<property name="Comment" column="[Comment]" type="String" not-null="false" length="16" />
		<property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
		<property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
		<property name="DateOfManufacture" column="[DateOfManufacture]" type="DateTime" not-null="false" length="8" />
		<property name="ValidDate" column="[ValidDate]" type="DateTime" not-null="false" length="8" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
		<many-to-one name="BEquipmentSuppliesType" class="BEquipmentSuppliesType" >
			<column name="[ESTId]" not-null="false"/>
		</many-to-one>
		<many-to-one name="HREmployee" class="HREmployee" >
			<column name="[EmpID]" not-null="false"/>
		</many-to-one>
		<bag name="MEEquipmentSuppliesUseInfoList" inverse="true" cascade="all-delete-orphan" lazy="true">
			<key>
                <column name="[ESRFId]" />
            </key>
			<one-to-many class="MEEquipmentSuppliesUseInfo"/>
		</bag>
	</class>
</hibernate-mapping>
