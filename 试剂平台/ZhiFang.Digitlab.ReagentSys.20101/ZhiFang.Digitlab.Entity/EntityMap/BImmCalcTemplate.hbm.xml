<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
  <class name="BImmCalcTemplate" table="[dbo].[B_ImmCalcTemplate]">
    <id name="Id" column="[CalcFormulaID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="CalcFormula" column="[CalcFormula]" type="String" not-null="false" length="60" />
    <property name="CalcFormulaShortName" column="[CalcFormulaShortName]" type="String" not-null="false" length="60" />
    <property name="ReMarke" column="[ReMarke]" type="String" not-null="false" length="200" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="IsUsed" column="[IsUsed]" type="Int32" not-null="false" length="4" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <bag name="BImmDxCalcRuleList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[CalcFormulaID]" />
      </key>
      <one-to-many class="BImmDxCalcRule"/>
    </bag>
    <bag name="BImmDxCalcVarReplaceList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[CalcFormulaID]" />
      </key>
      <one-to-many class="BImmDxCalcVarReplace"/>
    </bag>
    <bag name="BImmItemCalcInfoList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[CalcFormulaID]" />
      </key>
      <one-to-many class="BImmItemCalcInfo"/>
    </bag>
  </class>
</hibernate-mapping>
