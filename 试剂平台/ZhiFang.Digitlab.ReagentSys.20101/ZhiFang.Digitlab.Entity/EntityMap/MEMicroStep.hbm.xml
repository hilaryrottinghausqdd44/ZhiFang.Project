<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
	<class name="MEMicroStep" table="[dbo].[ME_MicroStep]">
		<id name="Id" column="[MicroStepID]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
		<property name="ReportValue" column="[ReportValue]" type="String" not-null="false" length="500" />
    <property name="RecordItemValue" column="[RecordItemValue]" type="String" not-null="false" length="500" />
		<property name="ResultStatus" column="[ResultStatus]" type="String" not-null="false" length="10" />
		<property name="AlarmLevel" column="[AlarmLevel]" type="Int32" not-null="false" length="4" />
		<property name="IsReport" column="[isReport]" type="Boolean" not-null="false" length="1" />
		<property name="ResultComment" column="[ResultComment]" type="String" not-null="false" length="16" />
		<property name="ESampleNo" column="[ESampleNo]" type="String" not-null="false" length="20" />
		<property name="BarcodeNo" column="[BarcodeNo]" type="String" not-null="false" length="20" />
		<property name="ReportFlag" column="[ReportFlag]" type="Int32" not-null="false" length="4" />
		<property name="BeginTime" column="[BeginTime]" type="DateTime" not-null="false" length="8" />
		<property name="TestTime" column="[TestTime]" type="DateTime" not-null="false" length="8" />
		<property name="TestFlag" column="[TestFlag]" type="Boolean" not-null="false" length="1" />
		<property name="AlarmTime" column="[AlarmTime]" type="DateTime" not-null="false" length="8" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
		<many-to-one name="BMicroTestStep" class="BMicroTestStep" lazy="false">
			<column name="[MicroTestStepID]" not-null="false"/>
		</many-to-one>
		<many-to-one name="MEGroupSampleItem" class="MEGroupSampleItem" lazy="false">
			<column name="[ResultID]" not-null="false"/>
		</many-to-one>
		<many-to-one name="BMicro" class="BMicro" lazy="false">
			<column name="[MicroID]" not-null="false"/>
		</many-to-one>
		<bag name="MEMicroStepInfoList" inverse="true" cascade="all-delete-orphan" lazy="true">
			<key>
                <column name="[MicroStepID]" />
            </key>
			<one-to-many class="MEMicroStepInfo"/>
		</bag>
		<bag name="MEMicroDSTValueList" inverse="true" cascade="all-delete-orphan" lazy="true">
			<key>
                <column name="[MicroStepID]" />
            </key>
			<one-to-many class="MEMicroDSTValue"/>
		</bag>
		<bag name="MEMicroStepBacodeList" inverse="true" cascade="all-delete-orphan" lazy="true">
			<key>
                <column name="[MicroStepID]" />
            </key>
			<one-to-many class="MEMicroStepBacode"/>
		</bag>
	</class>
</hibernate-mapping>
