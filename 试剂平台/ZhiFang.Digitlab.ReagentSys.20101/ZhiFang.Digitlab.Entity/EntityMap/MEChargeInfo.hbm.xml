    <many-to-one name="MEGroupSampleForm" class="MEGroupSampleForm" lazy="false">
      <column name="[GroupSampleFormID]" not-null="false"/>
    </many-to-one>
    <many-to-one name="MEPTOrderForm" class="MEPTOrderForm" lazy="false">
      <column name="[OrderFormID]" not-null="false"/>
    </many-to-one>
  </class>
</hibernate-mapping>
