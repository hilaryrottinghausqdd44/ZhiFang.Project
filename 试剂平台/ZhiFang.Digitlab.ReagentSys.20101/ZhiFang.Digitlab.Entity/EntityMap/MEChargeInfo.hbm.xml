    <property name="AmountReceivable" column="[AmountReceivable]" type="Decimal" not-null="false" length="9" />
    <property name="RealAmount" column="[RealAmount]" type="Decimal" not-null="false" length="9" />
    <property name="OperType" column="[OperType]" type="Int32" not-null="false" length="4" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
