<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
	<class name="BSampleStatus" table="[dbo].[B_SampleStatus]">
		<id name="Id" column="[SampleStatusID]" type="Int64">
			<generator class="assigned" />
		</id>
        <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
		<property name="OperateObjectID" column="[OperateObjectID]" type="Int64" not-null="false" length="8" />
		<property name="Comment" column="[Comment]" type="String" not-null="false" length="8000" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<many-to-one name="BOperateObjectType" class="BOperateObjectType" lazy="false">
			<column name="[OperateObjectTypeID]" not-null="false"/>
		</many-to-one>
		<many-to-one name="BSampleStatusType" class="BSampleStatusType" lazy="false">
			<column name="[SampleStatusTypeID]" not-null="false"/>
		</many-to-one>
	</class>
</hibernate-mapping>
