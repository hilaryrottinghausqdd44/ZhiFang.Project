<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
	<class name="OtherAttachment" table="[dbo].[Other_Attachment]">
		<id name="Id" column="[AttachmentID]" type="Int64">
			<generator class="assigned" />
		</id>
		<version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
		<property name="RecordIndex" column="[RecordIndex]" type="Int64" not-null="false" length="8" />
		<property name="Name" column="[Name]" type="String" not-null="false" length="128" />
		<property name="Url" column="[Url]" type="String" not-null="false" length="500" />
		<property name="Creator" column="[Creator]" type="String" not-null="false" length="20" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
		<property name="Modifier" column="[Modifier]" type="String" not-null="false" length="20" />
		<property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <property name="CreatorID" column="[CreatorID]" type="Int64" not-null="false" length="8" />
    <property name="ModifierID" column="[ModifierID]" type="Int64" not-null="false" length="8" />
    <property name="FileTypeID" column="[FileTypeID]" type="Int64" not-null="false" length="8" />
    <many-to-one name="BTDAppComponent" class="BTDAppComponents" lazy="false">
			<column name="[AppComID]" />
		</many-to-one>
	</class>
</hibernate-mapping>
