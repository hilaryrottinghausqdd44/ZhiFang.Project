<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
	<class name="OtherReturn" table="[dbo].[Other_Return]">
		<id name="Id" column="[ReturnID]" type="Int64">
			<generator class="assigned" />
		</id>
		<version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
		<property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
		<property name="RecordID" column="[RecordID]" type="Int64" not-null="false" length="8" />
		<property name="ParReturnID" column="[ParReturnID]" type="Int64" not-null="false" length="8" />
		<property name="Title" column="[Title]" type="String" not-null="false" length="512" />
		<property name="Content" column="[Content]" type="String" not-null="false" length="16" />
		<property name="MaxNum" column="[MaxNum]" type="Int32" not-null="false" length="4" />
		<property name="Creator" column="[Creator]" type="String" not-null="false" length="20" />
		<property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />    
    <property name="CreatorID" column="[CreatorID]" type="Int64" not-null="false" length="8" />
    <many-to-one name="BTDAppComponent" class="BTDAppComponents" lazy="false">
			<column name="[AppComID]" />
		</many-to-one>
	</class>
</hibernate-mapping>
