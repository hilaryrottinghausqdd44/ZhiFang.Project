<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Digitlab.Entity" assembly="ZhiFang.Digitlab.Entity">
  <class name="BMicroBloodCultureBottleType" table="[dbo].[B_MicroBloodCultureBottleType]">
    <id name="Id" column="[MicroBCBTID]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="true" length="8" />
    <property name="CName" column="[CName]" type="String" not-null="false" length="50" />
    <property name="EName" column="[EName]" type="String" not-null="false" length="50" />
    <property name="SName" column="[SName]" type="String" not-null="false" length="50" />
    <property name="Shortcode" column="[Shortcode]" type="String" not-null="false" length="20" />
    <property name="PinYinZiTou" column="[PinYinZiTou]" type="String" not-null="false" length="50" />
    <property name="LogoColor" column="[LogoColor]" type="Int32" not-null="false" length="4" />
    <property name="LogoPicture" column="[LogoPicture]" type="Byte[]" not-null="false" length="16" />
    <property name="BottleType" column="[BottleType]" type="Int32" not-null="false" length="4" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="DataUpdateTime" column="[DataUpdateTime]" type="DateTime" not-null="false" length="8" />
    <many-to-one name="BCultureMedium" class="BCultureMedium" >
      <column name="[CultureMediumID]" not-null="false"/>
    </many-to-one>
    <bag name="BMicroBCBottleTypeOfItemList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[MicroBCBTID]" />
      </key>
      <one-to-many class="BMicroBCBottleTypeOfItem"/>
    </bag>
    <bag name="BMicroBCBottleTypeOfManufacturerList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[MicroBCBTID]" />
      </key>
      <one-to-many class="BMicroBCBottleTypeOfManufacturer"/>
    </bag>
    <bag name="MEMicroBCBottleManageInfoList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[MicroBCBTID]" />
      </key>
      <one-to-many class="MEMicroBCBottleManageInfo"/>
    </bag>
  </class>
</hibernate-mapping>
