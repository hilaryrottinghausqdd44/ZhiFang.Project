    </many-to-one>
    <many-to-one name="SampleFrom" class="MEPTSampleForm" >
      <column name="[SampleFormID]" not-null="false"/>
    </many-to-one>
    <bag name="MEGroupSampleItemList" inverse="true" cascade="all-delete-orphan" lazy="true">
      <key>
        <column name="[SampleItemID]" />
      </key>
      <one-to-many class="MEGroupSampleItem"/>
    </bag>
	</class>
</hibernate-mapping>
