  <spring xmlns="http://www.springframework.net">
    <parsers>
      <parser type="Spring.Data.Config.DatabaseNamespaceParser, Spring.Data" />
      <parser type="Spring.Transaction.Config.TxNamespaceParser, Spring.Data" />
    </parsers>
    <context>
      <resource uri="config://spring/objects" />
      <!--数据库访问层配置-->
      <resource uri="assembly://ZhiFang.ObjectFactory.WebAssist/ZhiFang.ObjectFactory.WebAssist.Config/DaoBase.xml" />
      <resource uri="assembly://ZhiFang.ObjectFactory.WebAssist/ZhiFang.ObjectFactory.WebAssist.Config/Dao.xml" />
      <!--业务处理层配置-->
      <resource uri="assembly://ZhiFang.ObjectFactory.WebAssist/ZhiFang.ObjectFactory.WebAssist.Config/ServiceBase.xml" />
      <resource uri="assembly://ZhiFang.ObjectFactory.WebAssist/ZhiFang.ObjectFactory.WebAssist.Config/Service.xml" />
      <!--契约实现-->
      <resource uri="assembly://ZhiFang.ObjectFactory.WebAssist/ZhiFang.ObjectFactory.WebAssist.Config/Facade.xml" />
    </context>
    <objects xmlns="http://www.springframework.net" xmlns:aop="http://www.springframework.net/aop">
    
      <!--数据库升级-->
      <object id="UpdateDataBase" type="Spring.Aop.Support.SdkRegularExpressionMethodPointcut, Spring.Aop">
        <property name="Patterns">
          <list>
            <value>ZhiFang.WebAssist.RBACService.*</value>
          </list>
        </property>
      </object>
      <!--拦截服务层所有业务操作-->
      <object id="LoginValidationOperation" type="Spring.Aop.Support.SdkRegularExpressionMethodPointcut, Spring.Aop">
        <property name="Patterns">
          <list>
            <value>ZhiFang.WebAssist.WeiXinAppService.*</value>
            <value>ZhiFang.WebAssist.SingleTableService.*</value>
            <value>ZhiFang.WebAssist.ConstructionService.*</value>
            <value>ZhiFang.WebAssist.PublicService.*</value>
          </list>
        </property>
      </object>
      <!--登录成功后,实验室数据升级RBAC_BA_Login-->
      <object id="LoginAfterOperation" type="Spring.Aop.Support.SdkRegularExpressionMethodPointcut, Spring.Aop">
        <property name="Patterns">
          <list>
            <value>ZhiFang.WebAssist.RBACService.*</value>
          </list>
        </property>
      </object>
      <object id="UpdateDataBaseAdvice" type="ZhiFang.Interceptor.WebAssist.UpdateDataBaseAdvice, ZhiFang.Interceptor.WebAssist" singleton="false" />
      <!--登录验证切面-->
      <object id="LoginValidationAdvice" type="ZhiFang.Interceptor.WebAssist.LoginValidationAdvice, ZhiFang.Interceptor.WebAssist" singleton="false" />
      <object id="ModuleValidationAdvice" type="ZhiFang.Interceptor.WebAssist.ModuleValidationAdvice, ZhiFang.Interceptor.WebAssist" singleton="false" />
      <!--登录成功后拦截-->
      <object id="LoginAfterFilterAdvice" type="ZhiFang.Interceptor.WebAssist.LoginAfterFilterAdvice, ZhiFang.Interceptor.WebAssist" singleton="false" />
      <!--关联切面-->
      <aop:config>
        <aop:advisor pointcut-ref="UpdateDataBase" advice-ref="UpdateDataBaseAdvice" />
        <aop:advisor pointcut-ref="LoginValidationOperation" advice-ref="LoginValidationAdvice" />
        <aop:advisor pointcut-ref="LoginValidationOperation" advice-ref="ModuleValidationAdvice" />
        <aop:advisor pointcut-ref="LoginAfterOperation" advice-ref="LoginAfterFilterAdvice" />
      </aop:config>
    </objects>
  </spring>
