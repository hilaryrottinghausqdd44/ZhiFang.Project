<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.WebAssist" assembly="ZhiFang.Entity.WebAssist">
  <class name="SCRecordItemLink" table="[dbo].[SC_RecordItemLink]">
    <id name="Id" column="[RecordLinkId]" type="Int64">
      <generator class="assigned" />
    </id>
    <version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />
    <property name="LabID" column="[LabID]" type="Int64" not-null="false" length="8" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
    <property name="IsUse" column="[IsUse]" type="Boolean" not-null="false" length="1" />
    <property name="DataAddTime" column="[DataAddTime]" type="DateTime" not-null="false" length="8" />
    <property name="TestItemCode" column="[TestItemCode]" type="String" not-null="false" length="50" />
    <property name="IsBillVisible" column="[IsBillVisible]" type="Boolean" not-null="false" length="1" />
    
    <many-to-one name="SCRecordType" class="SCRecordType" >
      <column name="[RecordTypeID]" not-null="true"/>
    </many-to-one>
    <many-to-one name="SCRecordTypeItem" class="SCRecordTypeItem" >
      <column name="[RecordTypeItemID]" not-null="true"/>
    </many-to-one>
  </class>
</hibernate-mapping>
