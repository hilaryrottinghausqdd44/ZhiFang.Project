<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZhiFang.Entity.WebAssist" assembly="ZhiFang.Entity.WebAssist">
  <class name="Employee" table="[dbo].[Employee]">
    <id name="Id" column="[EmployeeNo]" type="Int32">
      <generator class="assigned" />
    </id>
    <!--<version name="DataTimeStamp" column="[DataTimeStamp]" type="BinaryBlob" generated="always" unsaved-value="null" />-->
    <property name="CName" column="[CName]" type="String" not-null="false" length="30" />
    <property name="ShortCode" column="[ShortCode]" type="String" not-null="false" length="30" />
    <property name="EmployeeCode" column="[EmployeeCode]" type="String" not-null="false" length="30" />
    <property name="Gender" column="[Gender]" type="Int32" not-null="false" length="4" />
    <property name="Brithday" column="[Brithday]" type="DateTime" not-null="false" length="8" />
    <property name="DeptNo" column="[DeptNo]" type="Int32" not-null="false" length="4" />
    <property name="IsVisible" column="[isVisible]" type="Int32" not-null="false" length="4" />
    <property name="DispOrder" column="[DispOrder]" type="Int32" not-null="false" length="4" />
  </class>
</hibernate-mapping>
